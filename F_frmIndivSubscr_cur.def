Function IsThereQCD(strSSN As String) As Boolean
On Error GoTo Err_IsThereQCD

    Dim db As DAO.Database
    Dim rs As DAO.Recordset
    Dim strSQL As String
    strSQL = "SELECT tblSubscr.* FROM tblSubscr WHERE (((tblSubscr.SUBssn)='" & strSSN & "'));"
    
    Set db = CurrentDb()
        
    Set rs = db.OpenRecordset(strSQL, dbOpenSnapshot)
    If rs.RecordCount > 0 Then
        IsThereQCD = True
    Else
        IsThereQCD = False
    End If

    rs.Close
    db.Close
    Set db = Nothing

Exit_IsThereQCD:
    Exit Function

Err_IsThereQCD:
    'MsgBox "Error no. " & err & crlf & crlf & "The following error occurred:  " & Error$
    Resume Exit_IsThereQCD
End Function
