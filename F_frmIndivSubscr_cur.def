Private Sub Form_BeforeUpdate(intCancel As Integer)

  On Error GoTo Err_Form_BeforeUpdate
  
    Dim intDepCount As Integer
    Dim strSSN As String
    Dim intCoverID As String
    Dim strCustKey As String
    

    'Check to see if there is a GeoID on this Individual Subscriber.
'    If IsNull(Me![SUBgeoID]) Then
'        MsgBox "Please enter a GeoID!"
'        intCancel = True
'        GoTo Exit_Form_BeforeUpdate
'    End If

    'Check to see that the number of dependents is consistent with the plan type
    With Me
        intCoverID = Nz(.cboSubCoverID)
        strSSN = Nz(Me![SUBssn])
        intDepCount = Nz(CountDependents(strSSN))
    End With
    
    Select Case intCoverID
        Case 1
            If intDepCount <> 0 Then
                MsgBox "The number of dependents and the plan type don't match."
                intCancel = True
                Forms![frmIndivSubscr]![SUBPLANid].SetFocus
            End If
        Case 2
            If intDepCount <> 1 Then
                MsgBox "The number of dependents and the plan type don't match."
                intCancel = True
                Forms![frmIndivSubscr]![SUBPLANid].SetFocus
            End If
        Case 4
            If intDepCount < 2 Then
                MsgBox "The number of dependents and the plan type don't match."
                intCancel = True
                Forms![frmIndivSubscr]![SUBPLANid].SetFocus
            End If
    End Select

Exit_Form_BeforeUpdate:
    Exit Sub

Err_Form_BeforeUpdate:
    ' Display error information.
    MsgBox "Error number " & Err.Number & ": " & Err.DESCRIPTION
    GoTo Exit_Form_BeforeUpdate

End Sub
Private Sub Form_Current()

On Error GoTo Err_Form_Current

    Me.Filter = "SubStatus = 'INDIV'"
    'set the new record variable
    fNewRec = Me.NewRecord

    Me![AlphaControl] = Null
    Me![ClientPickList] = Null
    
    With Me
        If .txtSubCanceled = 2 Then
            ' On Hold yellow
            .SUBfirstNAME.BackColor = vbYellow
            .SUBlastNAME.BackColor = vbYellow
            .SUBmiddleNAME.BackColor = vbYellow
        ElseIf .txtSubCanceled = 3 Then
            ' Cancelled to Red
            .SUBfirstNAME.BackColor = vbRed
            .SUBlastNAME.BackColor = vbRed
            .SUBmiddleNAME.BackColor = vbRed
            '.ctlSUBcancelled.BackColor = vbRed
        Else
            ' Active green
            .SUBfirstNAME.BackColor = vbGreen
            .SUBlastNAME.BackColor = vbGreen
            .SUBmiddleNAME.BackColor = vbGreen
            '.ctlSUBcancelled.BackColor = 16777215
        End If
        .Repaint
    End With
    
    'check to see if there is a Primary Agent on this Individual Subscriber.
    'If Not IsNull(Forms![frmIndivSubscr]![SUBssn]) Then
    '    fScroll = True
    '    If CheckInvPriAgt(Me.SubID) = False Then
    '        MsgBox "Please enter a Primary Agent!"
    '        DoCmd.CancelEvent
    '        GoTo Exit_Form_Current
    '    End If
    'End If
    
    'if this is not a new record
    'then populate the std rate control
    If fNewRec = False Then
        Call cboSubCoverID_AfterUpdate
    End If

Exit_Form_Current:
    Exit Sub

Err_Form_Current:
    MsgBox Error$
    Resume Exit_Form_Current

End Sub


