Private Sub SUBcardPRTdte_BeforeUpdate(Cancel As Integer)
    If ValidateDateFormat(Me.SUBcardPRTdte) = False Then
        MsgBox "Please correct the Date Printed...it is not in the proper range." & vbCrLf & strMsg, vbCritical, "Validation Failed"
        Cancel = True
    End If
End Sub
