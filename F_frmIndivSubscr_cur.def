Public Sub btnDelete_Click()

On Error GoTo Err_btnDelete_Click
    
    Dim strCustKey As String
    Dim intAns As Integer
    
    intAns = MsgBox("Are you sure you want to permanently delete this subscriber?", vbExclamation + vbYesNo, "Confirm Delete")
    If intAns = vbNo Then
        Exit Sub
    End If
    
    Dim db As DAO.Database
    Set db = CurrentDb()
    
    strCustKey = Me![PLTcustKEY]

    DoCmd.Hourglass True

    If CheckforOpenInvoices(strCustKey) Then
        DoCmd.Hourglass False
        MsgBox ("There are Platinum invoices out there.  You can't delete this customer!")
        GoTo Exit_btnDelete_Click
    Else
        
        mstrVarRetVal = DeleteARCust(strCustKey)
        
        DoCmd.SetWarnings False
        DoCmd.Echo False
        db.Execute ("DELETE tblSubscr.*, tblSubscr.PLTcustKEY FROM tblSubscr WHERE (((tblSubscr.PLTcustKEY)='" & strCustKey & "'));")
        Me.Requery
        DoCmd.Echo True
        DoCmd.SetWarnings True
        DoCmd.Hourglass False
        MsgBox "Customer was deleted!"
        
    End If
    
    db.Close
    Set db = Nothing

Exit_btnDelete_Click:
    DoCmd.Hourglass False
    Exit Sub

Err_btnDelete_Click:
    If Err = 2501 Then
        Resume Next
    End If

    DoCmd.Hourglass False
    MsgBox Error$
    Resume Exit_btnDelete_Click

End Sub
Private Sub cboSubCoverID_AfterUpdate()

On Error GoTo Err_cboSubCoverID_AfterUpdate

    Dim db As DAO.Database
    Dim rs As DAO.Recordset
    Dim strSQL As String

    Dim strGroupID As String
    Dim intPlanID As Integer
    Dim intCoverID As Integer
    
    Set db = CurrentDb()

    With Me
        strGroupID = .cboGroupID
        intPlanID = .cboSubPlanID
        intCoverID = .cboSubCoverID
    End With
    
    strSQL = "SELECT tblRates.GroupID, tblRates.PlanID, " & _
                "tblRates.CoverID " & _
                "FROM tblRates " & _
                "WHERE (((tblRates.GroupID)='" & strGroupID & "') " & _
                "AND ((tblRates.PlanID)= " & intPlanID & ") " & _
                "AND ((tblRates.CoverID)= " & intCoverID & "));"
                
    Set rs = db.OpenRecordset(strSQL, dbOpenSnapshot)
    
    If rs.RecordCount > 0 Then
    
    With Me
        'populate no of dependents
        .SUBnoDEPENT = CountDependents(Me.SUBssn)
        If .cboSubCoverID <> "" And .cboSubPlanID <> "" And .cboGroupID <> "" _
        Or Not IsNull(.cboSubCoverID) And IsNull(.cboSubPlanID) And Not IsNull(.cboGroupID) Then
            'populate standard rate control
            .SubStdRate = FindRate(strGroupID, intCoverID, intPlanID)
            .RateID = lngRateID
        Else
            .SubStdRate = 0
            .RateID = 0
        End If
    End With
    
    End If
    
    rs.Close
    Set rs = Nothing
    db.Close
    Set db = Nothing
    
Exit_cboSubCoverID_AfterUpdate:
    Exit Sub

Err_cboSubCoverID_AfterUpdate:
    ' Display error information.
    MsgBox "Error number " & Err.Number & ": " & Err.DESCRIPTION
    GoTo Exit_cboSubCoverID_AfterUpdate

End Sub
