Private Sub SUBfirstNAME_AfterUpdate()
'Build the look up name
    Me![SUB_LUname] = Me![SUBlastNAME] & ", " & Me![SUBfirstNAME] & " " & Me![SUBmiddleNAME]
End Sub
Private Sub SUBmiddleNAME_AfterUpdate()

 'Build the look up name
    Me![SUB_LUname] = Me![SUBlastNAME] & ", " & Me![SUBfirstNAME] & " " & Me![SUBmiddleNAME]


End Sub
Private Sub cmdChangeStatus_Click()

On Error GoTo Err_cmdChangeStatus_Click

    Dim stDocName As String
    Dim stLinkCriteria As String

    stDocName = "fdlgChangeIndivStatus"
    DoCmd.OpenForm stDocName, , , stLinkCriteria
    
    With Forms(stDocName)
        .txtID = Me.SUBssn
        .txtForm = Me.Name
    End With

Exit_cmdChangeStatus_Click:
    Exit Sub

Err_cmdChangeStatus_Click:
    MsgBox Err.DESCRIPTION
    Resume Exit_cmdChangeStatus_Click
    
End Sub