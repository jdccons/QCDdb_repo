Private Sub btnCreateGroup_Click()

On Error GoTo PROC_ERR

    Dim GROUPid As String
    Dim CoverageType As String
    Dim intCoverageType As Integer
    Dim GroupType As String
    Dim intGroupType As Integer
    Dim Password As String
    Dim fCreateGroup As Boolean
    
    fCreateGroup = False
            
    'val GroupID
    If IsNull(Me.cboGroupID) Then
        MsgBox "You must populate the GroupID."
        Me.cboGroupID.SetFocus
        fCreateGroup = False
        GoTo PROC_EXIT
    Else
        GROUPid = Me.cboGroupID
        fCreateGroup = True
    End If
    
    'is group already setup on website
    Dim Group As clsWebGroup
    Set Group = New clsWebGroup
    Dim fFound As Boolean
    fFound = Group.IsGroup(GROUPid)
    Set Group = Nothing
    If fFound Then
        MsgBox "The group already exists on the website."
        With Me
            .cboGroupID = Null
            .cboCoverageType = Null
            .cboGroupType = Null
            .txtPassword = Null
        End With
        fCreateGroup = False
        GoTo PROC_EXIT
    End If
    
    'val coveragetype
    If IsNull(Me.cboCoverageType) Then
        MsgBox "You must populate the Coverage Type."
        Me.cboCoverageType.SetFocus
        fCreateGroup = False
        GoTo PROC_EXIT
    Else
        CoverageType = Me.cboCoverageType
        If CoverageType = "Three Tier" Then
            intCoverageType = 3
        ElseIf CoverageType = "Four Tier" Then
            intCoverageType = 4
        End If
        fCreateGroup = True
    End If
    
    'val grouptype
    If IsNull(Me.cboGroupType) Then
        MsgBox "You must populate the Group Type."
        Me.cboGroupType.SetFocus
        fCreateGroup = False
        GoTo PROC_EXIT
    Else
        GroupType = Me.cboGroupType
        If GroupType = "QCD Only" Then
            intGroupType = 1
        ElseIf GroupType = "All American" Then
            intGroupType = 4
        End If
        fCreateGroup = True
    End If
    
    'val password
    If IsNull(Me.txtPassword) Then
        MsgBox "You must populate the Password."
        Me.txtPassword.SetFocus
        fCreateGroup = False
        GoTo PROC_EXIT
    Else
        Password = Me.txtPassword
        fCreateGroup = True
    End If
    
    DoCmd.Hourglass True
    DoCmd.SetWarnings False
    
    'add the group to the website
    If fCreateGroup = True Then
        ExecuteSQL "EXEC uspCreateWebGroup '" & GROUPid & "', " & intCoverageType & ", " & intGroupType & ", '" & Password & "';"
        MsgBox "Your group has been created on the website."
        With Me
            .cboGroupID = Null
            .cboCoverageType = Null
            .cboGroupType = Null
            .txtPassword = Null
        End With
    Else
        MsgBox "Your group was not created on the website."
    End If
    
    DoCmd.Hourglass False
    DoCmd.SetWarnings True
    
PROC_EXIT:
    Exit Sub

PROC_ERR:
    MsgBox Err.DESCRIPTION
    Resume PROC_EXIT

End Sub
Private Sub btnExit_Click()

On Error GoTo Err_btnExit_Click

    DoCmd.Close

Exit_btnExit_Click:
    Exit Sub

Err_btnExit_Click:
    MsgBox Err.DESCRIPTION
    Resume Exit_btnExit_Click
    
End Sub