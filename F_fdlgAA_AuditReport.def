Private Sub cmdAuditReport_Click()
    
    On Error GoTo Err_cmdAuditReport_Click
    
    Dim fCheck As Boolean

    DoCmd.SetWarnings False
    If IsReportOpen("rptAA_AuditReport") = True Then
        DoCmd.Close acReport, "rptAA_AuditReport", acSaveNo
    End If
    
    ' Clear the temp table
    ExecuteSQL ("UPDATE tmpAA_AuditReport " & _
            "SET tmpAA_AuditReport.[Count] = 0, tmpAA_AuditReport.Rate = 0, " & _
            "tmpAA_AuditReport.Premium = 0 " & _
            "FROM tmpAA_AuditReport;")
    'DoCmd.OpenQuery "qupd_ResetAA_AuditReport"
    
    ' Populate the temp table
    fCheck = PopulateAA_Audit(Me.CUST_KEY)
    
    If fCheck Then
        DoCmd.OpenReport "rptAA_AuditReport", acViewPreview
    Else
         GoTo Err_cmdAuditReport_Click
    End If
     
Exit_cmdAuditReport_Click:
    Exit Sub

Err_cmdAuditReport_Click:
    If fCheck = False Then
        MsgBox "The temporary table for the report did not populate correctly.  Contact technical support."
        Resume Exit_cmdAuditReport_Click
    Else
        MsgBox Err.DESCRIPTION
        Resume Exit_cmdAuditReport_Click
    End If
    
End Sub
Private Sub cmdClose_Click()

On Error GoTo Err_cmdClose_Click

    DoCmd.Close

Exit_cmdClose_Click:
    Exit Sub

Err_cmdClose_Click:
    MsgBox Err.DESCRIPTION
    Resume Exit_cmdClose_Click
    
End Sub

Private Sub CUST_KEY_AfterUpdate()

    Me.CustName = Me.CUST_KEY.Column(1)
    
End Sub

Private Sub Form_Current()

    Me.CUST_KEY.SetFocus
    
End Sub