Private Sub btnRun_Click()

    On Error GoTo Err_btnRun_Click
    
    Dim strSpec As String
    Dim strTableName As String
    Dim strFileName As String
    Dim strDate As String
    Dim FileDialog As clsFileDialog
    Dim Msg As String
    Dim Response As String
    
    Msg = "This process will take a long time. " & vbCrLf & _
                "Click OK if you want to continue. " & vbCrLf & _
                "Otherwise, click cancel to not run " & vbCrLf & _
                "the process at this time."
    Response = MsgBox(Msg, vbOKCancel, "Run long process?")
    If Response = vbOK Then
        DoCmd.Hourglass True
        DoCmd.SetWarnings False
        Call RebuildAvesisTable
    
        Set FileDialog = New clsFileDialog
       
        strDate = Format(Month(date), "00") & Format(Day(date), "00") & Format(Year(date), "0000")
    
        With FileDialog
            .DefaultExt = "txt"
            .DialogTitle = "Save Avesis Export file"
            .Filter = "Text files (*.txt)|*.txt|All Files (*.*)|*.*"
            .FilterIndex = 0
            'strFileName = "F:\jdcConsulting\Group Link\" & Me.ctlGroupID & "_AA_Export.txt"
            .FileName = "Avesis" & strDate & ".txt"
            .Flags = FleFileMustExist + FleHideReadOnly + FleCreatePrompt
            .hWndParent = 0 'Use your form's .hwnd property if available
            .MaxFileSize = 255
            
        strSpec = "Avesis"
        strTableName = "qryAvesis"
    
            If .Show(False) Then
                strFileName = .FileName
                DoCmd.TransferText acExportFixed, strSpec, strTableName, strFileName, 0
            Else
                MsgBox "User cancelled"
            End If
    
        End With
    
        Set FileDialog = Nothing
        
        DoCmd.SetWarnings True
        DoCmd.Hourglass False
        MsgBox "File created!."
    
    End If
   
Exit_btnRun_Click:
    Exit Sub

Err_btnRun_Click:
    MsgBox Err.DESCRIPTION
    Resume Exit_btnRun_Click
    
End Sub