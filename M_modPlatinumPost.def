Function IsTranNo(strTranNo As String) As Boolean
'------------------------------------------------
'Purpose:  Tests for a tranno in ARHDR
'------------------------------------------------
'Programmer:  John Criswell
'Create Date:  9/22/99
'Update Date:  John Criswell
'Updated by:  5/17/2005 - updated code to work with new version of PFW (ver 5.3)
    
On Error GoTo PROC_ERR

Dim db As DAO.Database
Dim rs As DAO.Recordset

Dim strSQL As String

strSQL = "SELECT ARHDR.TransactionNumber " & _
           "FROM ARHDR " & _
           "WHERE (((ARHDR.TransactionNumber)='" & strTranNo & "'));"

Set db = CurrentDb()
Set rs = db.OpenRecordset(strSQL, dbOpenDynaset, dbSeeChanges)

If rs.RecordCount > 0 Then
    IsTranNo = True
Else
    IsTranNo = False
End If

rs.Close
db.Close
Set db = Nothing

Exit Function

PROC_ERR:
  MsgBox "Error no. " & Err & crlf & crlf & "The following error occurred:  " & Error$
  Resume Next

End Function
Function DidThisInvoicePosttoPlat(strTranNo As String)
'------------------------------------------------
'Purpose:  Determines whether tranno from tblInvHdr
'          can be found in ARHDR.  If tranno in tblInvHdr
'          can be found in ARHDR, then the invoice was
'          successfully posted to Platinum.
'------------------------------------------------
'Programmer:  John Criswell
'Create Date:  9/22/99
'Update Date:
'Updated by:
Dim db As DAO.Database
Dim rs As DAO.Recordset
Dim strSQL As String

strSQL = "SELECT tblInvHdr.[TRANS NO] FROM tblInvHdr WHERE (((tblInvHdr.[TRANS NO])='" & strTranNo & "'));"
'Debug.Print strSQL
Set db = CurrentDb()

Set rs = db.OpenRecordset(strSQL, dbOpenSnapshot)
    If IsTranNo(rs("TRANS NO")) Then
        DidThisInvoicePosttoPlat = True
    Else
        DidThisInvoicePosttoPlat = False
    End If

Set db = Nothing

Exit Function

On Error GoTo PROC_ERR

PROC_ERR:
  MsgBox "Error no. " & Err & crlf & crlf & "The following error occurred:  " & Error$
  Resume Next

End Function
Function AreAnyInvoicesInPlat()
'------------------------------------------------
'Purpose:  First checks to see whether any new invoices in tblInvHdr
'          have already been posted to ARHDR through an error.
'------------------------------------------------
'Programmer:  John Criswell
'Create Date:  9/28/99
'Update Date:
'Updated by:

On Error GoTo FUNC_ERR

Dim db As DAO.Database
Dim rs As DAO.Recordset

Set db = CurrentDb()
Set rs = db.OpenRecordset("tblInvHdr", dbOpenSnapshot)
    Do Until rs.EOF
        If IsTranNo(rs("TRANS NO")) Then
           AreAnyInvoicesInPlat = True
           GoTo FUNC_EXIT
        Else
           AreAnyInvoicesInPlat = False
        End If
    rs.MoveNext
    Loop
rs.Close
db.Close

Set db = Nothing
   
Exit Function

FUNC_EXIT:
    rs.Close
    db.Close
    Exit Function

FUNC_ERR:
  MsgBox "Error no. " & Err & crlf & crlf & "The following error occurred:  " & Error$
  Resume FUNC_EXIT

End Function
Function DidAllInvoicesPosttoPlat()
'------------------------------------------------
'Purpose:  Determines whether ALL tranno's from tblInvHdr
'          can be found in ARHDR.  This will set the function
'          to false and exit when it encounters the first invoice
'          that did not post to Platinum (ARHDR).
'------------------------------------------------
'Programmer:  John Criswell
'Create Date:  9/22/99
'Update Date:
'Updated by:

On Error GoTo FUNC_ERR

Dim db As DAO.Database
Dim rs As DAO.Recordset

Set db = CurrentDb()
Set rs = db.OpenRecordset("tblInvHdr", dbOpenSnapshot)
    Do Until rs.EOF
        If IsTranNo(rs("TRANS NO")) Then
           DidAllInvoicesPosttoPlat = True
        Else
           DidAllInvoicesPosttoPlat = False
           GoTo FUNC_EXIT
        End If
    rs.MoveNext
    Loop
rs.Close
db.Close

Set db = Nothing
   
Exit Function

FUNC_EXIT:
    rs.Close
    db.Close
    Exit Function

FUNC_ERR:
  MsgBox "Error no. " & Err & crlf & crlf & "The following error occurred:  " & Error$
  Resume FUNC_EXIT

End Function
Function SaveNextSysDocID(lngCurrentSysDocID As Long) As Boolean
    
    On Error GoTo Err_SaveNextSysDocID
    
    Dim db As DAO.Database
    Dim strSQL As String
    Dim lngNextSysDocID As Long
    
    'increment the SysDocID
    lngNextSysDocID = lngCurrentSysDocID + 1
        
    'write back new sysdocid, userid, date, and time to ARNEXTSY
    Set db = CurrentDb()
    
    strSQL = "UPDATE ARNEXTSY SET ARNEXTSY.Nextsysdocid =" & lngNextSysDocID & ", "
    strSQL = strSQL & "ARNEXTSY.RecUserID = 'INVOICE', "
    strSQL = strSQL & "ARNEXTSY.RecDate = '" & Format(date, "mm/dd/yyyy") & "'"
    'strSQL = strSQL & "ARNEXTSY.RecTime = '" & Format(Time, "Long Time") & "';"
    
    db.Execute strSQL, dbSeeChanges
    db.Close
    Set db = Nothing
    
    SaveNextSysDocID = True
    
    Exit Function
    
Exit_SaveNextSysDocID:
    Exit Function

Err_SaveNextSysDocID:
  If Err = 3197 Then
    MsgBox "The Platinum table, ARNEXTSY, is locked.  Call for support."
    SaveNextSysDocID = False
    db.Close
  Else
    MsgBox "Error number " & Err.Number & ": " & Err.DESCRIPTION
  End If
  
  GoTo Exit_SaveNextSysDocID
    
End Function

Function GetCurrentSysDocID() As Long
'------------------------------------------------
'Purpose:  Retrieves the current SysDocID from ARNEXTSY
'------------------------------------------------
'Programmer:  John Criswell
'Date Created:  9/21/99
'Date Update:   11/9/99
'Updated by:  John Criswell

  On Error GoTo Err_GetCurrentSysDocID
    
    Dim lngCurrentSysDocID As Long
    Dim varRetVal As Variant
    Dim db As DAO.Database
    Dim rs As DAO.Recordset
            
    Set db = CurrentDb()
    Set rs = db.OpenRecordset("ARNEXTSY", dbOpenSnapshot)
       
    lngCurrentSysDocID = rs("NextSysDocID")
   
    rs.Close
    db.Close
    Set db = Nothing
    
    varRetVal = SaveNextSysDocID(lngCurrentSysDocID)
    
    If varRetVal Then
        GetCurrentSysDocID = lngCurrentSysDocID
    Else
        GetCurrentSysDocID = 0
    End If
    
  Exit Function

Exit_GetCurrentSysDocID:
    Exit Function
  
Err_GetCurrentSysDocID:
  
  MsgBox "Error number " & Err.Number & ": " & Err.DESCRIPTION
  GoTo Exit_GetCurrentSysDocID
  
End Function
