Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database   'Use database order for string comparisons
Option Explicit
Private Sub Form_BeforeInsert(intCancel As Integer)

  On Error GoTo PROC_ERR
    Me![Dent_LUName] = Me![DentLast] & ", " & Me![DentFirst] & " " & Me![DENTmiddleINIT]

  Exit Sub

PROC_ERR:
  MsgBox "The following error occured: " & Error$
  Resume Next
End Sub
Private Sub Form_BeforeUpdate(intCancel As Integer)

  On Error GoTo PROC_ERR
    Me![Dent_LUName] = Me![DentLast] & ", " & Me![DentFirst] & " " & Me![DENTmiddleINIT]
  Exit Sub

PROC_ERR:
  MsgBox "The following error occured: " & Error$
  Resume Next
End Sub
Private Sub Form_Current()
On Error GoTo Err_Form_Current

    Me![AlphaControl] = Null
    Me![DentPickList] = Null

Exit_Form_Current:
    Exit Sub

Err_Form_Current:
    MsgBox Error$
    Resume Exit_Form_Current

End Sub