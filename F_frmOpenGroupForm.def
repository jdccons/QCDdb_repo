Private Sub cmdOpenDataEntry_Click()
    
    'Purpose:  prompts for a password in order to open
    '          the group form in data entry mode
    '--------------------------------------------------------
    ' Parameters:  -
    ' Programmer:  John Criswell
    ' Create Date:  12/09/07
    ' Update Date:  12/09/07
    ' Updated by:  John Criswell
    ' Reason:
    '---------------------------------------------------------
    
    On Error GoTo ErrorHandler
    
    Dim fCheck As Boolean
    Dim stLinkCriteria As String
    
    If IsNull(Me.txtPassword) = True Or Me.txtPassword <> "Ch33se" Then
        MsgBox "The password is not correct. Please try again", vbExclamation, "QCD: Error"
        GoTo Exit_Point
    End If
    
    strForm = "frmGrp"
    DoCmd.OpenForm strForm, acNormal, , "[GRcancelled] = False", , acWindowNormal
    DoCmd.Close acForm, "frmOpenGroupForm", acSaveNo
    
Exit_Point:
    Exit Sub
    
ErrorHandler:
    MsgBox Err.DESCRIPTION & " " & Err.Number
    Resume Exit_Point

End Sub
