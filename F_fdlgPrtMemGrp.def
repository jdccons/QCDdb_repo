Private Sub PreviewButton_Click()

On Error GoTo Err_PreviewButton_Click

    Dim strWhere As String
    Dim strData As String
    strData = "qrptMemGrp"

    'group id
    If Not IsNull([c-GRPfrom]) Then
        If Not IsNull([c-GRPto]) Then
            strWhere = "[GroupID] BETWEEN '" & Me![c-GRPfrom] & "' AND '" & Me![c-GRPto] & "'"
        Else
            strWhere = "[GroupID] >= '" & Me![c-GEOfrom] & "'"
        End If
    ElseIf Not IsNull([c-GRPto]) Then
        strWhere = "[GroupID] <= '" & Me![c-GEOto] & "'"
    End If


    DoCmd.Hourglass True
    gintSortOrder = Me![OptSortBy]
    Me.Visible = False
    
    
      If DCount("*", strData, strWhere) = 0 Then
        MsgBox "The criteria you have chosen has resulted in no records." & vbCrLf & vbCrLf & "Please make another criteria selection and try again!"
        DoCmd.Hourglass False
        Me.Visible = True
    Exit Sub
    
 Else
    DoCmd.OpenReport "rptMemGrp", A_PREVIEW, , strWhere
    DoCmd.Hourglass False

 End If
 
Exit_PreviewButton_Click:
    Exit Sub

Err_PreviewButton_Click:
    MsgBox Error$
    DoCmd.SetWarnings True
    DoCmd.Hourglass False
    Resume Exit_PreviewButton_Click

End Sub