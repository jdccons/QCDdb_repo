Private Sub cboSubCoverage_AfterUpdate()
    
    'Purpose:  Find the plan rate based on coverage and put it in the rate control
    '--------------------------------------------------------
    ' Parameters:  -
    ' Programmer:  John Criswell
    ' Create Date:  12/13/07
    ' Update Date:  12/13/07
    ' Updated by:  John Criswell
    ' Reason:
    '---------------------------------------------------------

On Error GoTo Err_cboSubCoverage_AfterUpdate

    Dim strCoverage As String
    Dim intCoverID As Integer
    Dim intPlanID As Integer
    Dim strGroupID As String
    Dim strGroupPlanID As String

    'check to see if the rate ID for that subscriber exists on the rate table
    Dim db As DAO.Database
    Dim rs As DAO.Recordset
    Dim strSQL As String

    Set db = CurrentDb()
    With Me
        strGroupID = .SUBgroupID
        intPlanID = Nz(.cboPlan.Column(1))
        intCoverID = Nz(.cboSUBcoverage.Column(1))
    End With

    strSQL = "SELECT tblRates.GroupID, tblRates.PlanID, " & _
                "tblRates.CoverID " & _
                "FROM tblRates " & _
                "WHERE (((tblRates.GroupID)='" & strGroupID & "') " & _
                "AND ((tblRates.PlanID)= " & intPlanID & ") " & _
                "AND ((tblRates.CoverID)= " & intCoverID & "));"


    Set rs = db.OpenRecordset(strSQL, dbOpenSnapshot)
    If fDelete = True Then
        GoTo Exit_cboSubCoverage_AfterUpdate
    Else
        If rs.RecordCount > 0 Then
            With Me
                'populate no of dependents
                SUBnoDEPENT = DependentCheck_Bexar(Me.SUBssn)
                If .cboSUBcoverage <> "" And .cboPlan <> "" And .SUBgroupID <> "" _
                Or Not IsNull(.cboSUBcoverage) And IsNull(.cboPlan) And Not IsNull(.SUBgroupID) Then
                    'populate rate
                    .SUBrate = FindRate(strGroupID, intCoverID, intPlanID)
                    .RateID = lngRateID
                Else
                    .RateID = 0
                End If
            End With
            Call Form_BeforeUpdate(False)
        End If
    End If

    rs.Close
    Set rs = Nothing
    db.Close
    Set db = Nothing

Exit_cboSubCoverage_AfterUpdate:
    Exit Sub

Err_cboSubCoverage_AfterUpdate:
    MsgBox Error$
    Resume Exit_cboSubCoverage_AfterUpdate
    
End Sub

Private Sub Form_BeforeInsert(Cancel As Integer)

On Error GoTo Err_Form_BeforeInsert

    ' Assign a SubscriberID
    Me.SubID = AssignSubID()

Exit_Form_BeforeInsert:
    Exit Sub
    
Err_Form_BeforeInsert:
    MsgBox Error$
    
    Resume Exit_Form_BeforeInsert
End Sub
Private Sub Form_BeforeUpdate(Cancel As Integer)
    'Purpose:  checks number of dependents versus coverage
    '--------------------------------------------------------
    ' Parameters: Cancel -
    ' Programmer:  John Criswell
    ' Create Date:  12/12/07
    ' Update Date:  12/12/07
    ' Updated by:  John Criswell
    ' Reason:
    '---------------------------------------------------------
    
    
    On Error GoTo ErrorHandler
    
    
    Dim strCoverageCheck As String
    Dim intDepCount As Integer
    
    'Check to see that the number of dependents is consistent with the plan type
    strCoverageCheck = Nz(Me.cboSUBcoverage.Column(0))
    intDepCount = Nz(DependentCheck_Bexar(Me.SUBssn))
    
    Select Case strCoverageCheck
        Case "I"
            If intDepCount <> 0 Then
                MsgBox "The number of dependents and the plan type don't match."
                Cancel = True
                Me.cboSUBcoverage.SetFocus
            End If
        Case "O"
            If intDepCount <> 1 Then
                MsgBox "The number of dependents and the plan type don't match."
                Cancel = True
                Me.cboSUBcoverage.SetFocus
            End If
        Case "F"
            If intDepCount < 2 Then
                MsgBox "The number of dependents and the plan type don't match."
                Cancel = True
                Me.cboSUBcoverage.SetFocus
            End If
    End Select
    
    
Exit_Point:
    Exit Sub
    
ErrorHandler:
    MsgBox Err.DESCRIPTION & " " & Err.Number
    Resume Exit_Point
    
End Sub
Private Sub Form_Current()

    On Error GoTo Err_Form_Current

    Me![AlphaControl] = Null
    Me![SUBSCRPickList] = Null
    
    If Not IsNull(Me!SUBssn) Then
        Call cboSubCoverage_AfterUpdate
    End If
    
Exit_Form_Current:
    Exit Sub

Err_Form_Current:
    MsgBox Error$
    Resume Exit_Form_Current

End Sub

Private Sub fraCriteria_AfterUpdate()
    
    With Me
      If .fraCriteria = 1 Then
        .FindSSN.Visible = True
        .FindSUBID.Visible = False
        .FindSSN = ""
        .FindSUBID = ""
      Else
        .FindSSN.Visible = False
        .FindSUBID.Visible = True
        .FindSSN = ""
        .FindSUBID = ""
      End If
    End With
    
End Sub

Private Sub fsubGrpSubscrDep_Exit(Cancel As Integer)

On Error GoTo PROC_ERR
    
    If Forms![frmGrpSubscr_Bexar]![SUBnoDEPENT] <> DCount("[DEPcounter]", "tblDependent_Bexar", "[DEPsubID] = [SUBssn]") Then
        MsgBox "The Number of Dependents entered in the field above MUST equal the number of dependents listed here.  The No. Dep field above will be changed to " + Str$(DCount("[DEPcounter]", "tblDependent", "[DEPsubID] = [SUBssn]")) + "."
        Forms![frmGrpSubscr_Bexar]![SUBnoDEPENT] = DCount("[DEPcounter]", "tblDependent_Bexar", "[DEPsubID] = [SUBssn]")
    End If
    
    Exit Sub

PROC_ERR:
  MsgBox "The following error occured: " & Error$
  Resume Next

End Sub

