Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database

Private Sub cboGroupID_AfterUpdate()
    ' Update the RateID and Display Rate
    Me.SUBPLANid.Requery
    Call SUBPLANid_AfterUpdate
End Sub

Private Sub cmdSave_Click()

    ' Save the changes to the Subscriber
    With Me
        ExecuteSQL "UPDATE tblSubscr SET CoverID = " & .CoverID & " , RateID = " & .RateID & ", SUBgroupID = '" & .cboGroupID & "', SUBcontBEG = '" & .ctlSUBcontBEG & "', SUBcontEND = '" & .ctlSUBcontEND & "', SUBbankDraftNo = '" & .ctlSUBbankDraftNo & "' WHERE SUBssn = '" & .ctlSUBssn & "';"
        With Forms("frmIndivSubscrCoverage")
            .Requery
            .Refresh
        End With
    End With

End Sub
Private Sub SUBPLANid_AfterUpdate()

On Error GoTo Err_SUBPLANid_AfterUpdate

    Dim Subscr As clsSubscr
    Dim intCoverID As Integer
    Dim intPlanID As Integer
    Dim strGroupID As String
    Dim strGroupPlanID As String
    
    Dim intDepCount
    Dim strSSN As String
   
    Set Subscr = New clsSubscr
    
    With Me
      strSSN = Nz(.ctlSUBssn)
      Subscr.SSN = strSSN
      intDepCount = Subscr.DepCnt
      intCoverID = .SUBPLANid.Column(5)
      strGroupID = .cboGroupID
      .ctlSUBrate = FindRate(strGroupID, intCoverID, intPlanID)
      .RateID = lngRateID
    End With
    
    Set Subscr = Nothing
  
  Exit Sub

Exit_SUBPLANid_AfterUpdate:
    Exit Sub
    
Err_SUBPLANid_AfterUpdate:
    ' Display error information.
    MsgBox "Error number " & Err.Number & ": " & Err.DESCRIPTION
    GoTo Exit_SUBPLANid_AfterUpdate

End Sub