Option Compare Database

Private Sub cboGroupID_AfterUpdate()
    ' Update the RateID and Display Rate
    Me.SUBPLANid.Requery
    Call SUBPLANid_AfterUpdate
End Sub

Private Sub cmdSave_Click()

    On Error GoTo PROC_ERR
    
    Dim Subscr As clsSubscr
    Dim strSSN As String
    Dim strGroupID As String
    
    Set Subscr = New clsSubscr
    With Me
        strSSN = .ctlSUBssn
        strGroupID = .cboGroupID
    End With
    
    Call Subscr.LoadFromSSN(strSSN, strGroupID)
    
    With Subscr
            .LastName = Me.ctlSUBlastNAME
            .FirstName = Me.ctlSUBfirstNAME
            .MiddleInitial = Me.ctlSUBmiddleNAME
            .GROUPid = Me.cboGroupID
            .SubContBeg = Me.ctlSUBcontBEG
            .SubContEnd = Me.ctlSUBcontEND
            .BankDraftNo = Me.ctlSUBbankDraftNo
            .CoverID = Me.CoverID
            .RateID = Me.RateID
            .TransactionType = "UPDATE"
            .UserName = GetCurrentUserName()
            .DateUpdated = Date
    End With
    Call Subscr.StoreSub("Update", strSSN, strGroupID)
    Set Subscr = Nothing
    
    With Forms("frmIndivSubscrCoverage")
        .Requery
        .Refresh
    End With
    
PROC_EXIT:
  DoCmd.SetWarnings True
  DoCmd.Hourglass False
  Exit Sub


PROC_ERR:
    If Err.Number = 94 Then
        Resume Next
    Else
        MsgBox "Error: (" & Err.Number & ") " & Err.DESCRIPTION, vbCritical, "Procedure: frmUpdateIndividualSubscriber.cmdSave"
        Resume PROC_EXIT
    End If

End Sub

Private Sub Form_Load()
    Me.UserName = GetCurrentUserName()
End Sub

Private Sub SUBPLANid_AfterUpdate()

On Error GoTo Err_SUBPLANid_AfterUpdate

    Dim Subscr As clsSubscr
    Dim intCoverID As Integer
    Dim intPlanID As Integer
    Dim strGroupID As String
    Dim strGroupPlanID As String
    
    Dim intDepCount
    Dim strSSN As String
   
    Set Subscr = New clsSubscr
    
    With Me
      strSSN = Nz(.ctlSUBssn)
      Subscr.SSN = strSSN
      intDepCount = Subscr.DepCnt
      intCoverID = .SUBPLANid.Column(5)
      strGroupID = .cboGroupID
      .ctlSUBrate = FindRate(strGroupID, intCoverID, intPlanID)
      .RateID = lngRateID
    End With
    
    Set Subscr = Nothing
  
  Exit Sub

Exit_SUBPLANid_AfterUpdate:
    Exit Sub
    
Err_SUBPLANid_AfterUpdate:
    ' Display error information.
    MsgBox "Error number " & Err.Number & ": " & Err.DESCRIPTION
    GoTo Exit_SUBPLANid_AfterUpdate

End Sub