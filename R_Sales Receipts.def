Private Sub Report_Close()

    On Error GoTo PROC_ERR
    
    DoCmd.Restore
    
    If fIsLoaded("fdlgSalesReceipts") = True Then
        DoCmd.Close acForm, "fdlgSalesReceipts"
    End If
    
    If fIsLoaded("fmnuReportBilling") Then
        DoCmd.Close acForm, "fmnuReportBilling"
    End If
    
    If fIsLoaded("fmnuSwitchboard") = True Then
        Forms![fmnuSwitchboard].Visible = True
    End If
    
    Exit Sub
    
PROC_ERR:
  MsgBox "The following error occured: " & Error$
  Resume Next


End Sub

Private Sub Report_Open(Cancel As Integer)

    On Error GoTo Report_Open_Err
    
    If fIsLoaded("fdlgSalesReceipts") = True Then
        Forms!fdlgSalesReceipts.Visible = False
    End If
    
    If fIsLoaded("fmnuSwitchboard") = True Then
        Forms!fmnuSwitchboard.Visible = False
    End If
    
    Exit Sub
    
Report_Open_Exit:
    Exit Sub

Report_Open_Err:
      
    MsgBox "Error " & Err.Number & ": " & Err.DESCRIPTION
    GoTo Report_Open_Exit
    
    
    
End Sub