Private Sub btnCreateInvoice_Click()
'Purpose:  Populate subform with subscriber information
'Progammer:  Suzanne Johnsen
'Date Created:  9/21/99
'Date Updated:
'Updated By:

    On Error GoTo PROC_ERR
    
    SendKeys "+{ENTER}", True
    DoCmd.SetWarnings False
    DoCmd.OpenQuery "qryPopInvGrpDet_Bexar"  'qryPopInvGrpDet_Bexar
    Me.Refresh
    DoCmd.SetWarnings True
    
    Exit Sub

PROC_ERR:
  MsgBox "The following error occured: " & Error$
  Resume Next
End Sub

Private Sub btnPostPlt_Click()
'Purpose:  Posts a single group invoice to Platinum
On Error GoTo PROC_ERR
        
        Dim strTranNo As String
        strTranNo = Me![TRANS NO]
        
        DoCmd.Hourglass True
        DoCmd.SetWarnings False
        
        'append ARHDR
        DoCmd.OpenQuery "qappPostPltHdr_Bexar"
        
        'Check both Access and Platinum
        'to insure data was transferred correctly
        If DidThisInvoicePosttoPlat(strTranNo) Then
            'nothing
        Else
            MsgBox "This invoice " & strTranNo & " did not post to Platinum."
            GoTo PROC_EXIT
        End If
        
        'append ARLIN
        DoCmd.OpenQuery "qappPostPltDet_Bexar"
        
        'remove data from form
        DoCmd.OpenQuery "qdelDelInvHdr_Bexar"
        Forms![frmInvGrp_Bexar].Requery
        
        DoCmd.SetWarnings True
        DoCmd.Hourglass False
        
    Exit Sub
    
PROC_EXIT:
    DoCmd.SetWarnings True
    DoCmd.Hourglass False
    Exit Sub
    
PROC_ERR:
  MsgBox "The following error occured: " & Error$
  Resume PROC_EXIT
End Sub

Private Sub Form_AfterUpdate()

On Error GoTo Err_Form_AfterUpdate

    'Purpose:  Sets the current invoice date in tblConstants to the value on the form
    
    Dim db As DAO.Database
    Dim rs As DAO.Recordset
    Set db = CurrentDb()
    Set rs = db.OpenRecordset("tblConstants", dbOpenDynaset, dbSeeChanges)
    rs.Edit
    rs("FieldDateValue") = Me![INV DATE]
    rs.Update
    rs.Close
    db.Close
    Set db = Nothing

Exit_Form_AfterUpdate:
    Exit Sub

Err_Form_AfterUpdate:
    MsgBox Error$
    Resume Exit_Form_AfterUpdate


End Sub

Private Sub Form_Current()
    
    Dim db As DAO.Database
    Dim rs As DAO.Recordset
    Dim strSQL As String
    Dim strGeoID As String
    Dim strAgentID1 As String
    Dim strAgentID2 As String
    
    Set db = CurrentDb()
    Set rs = db.OpenRecordset("tblGrp_Bexar", dbOpenSnapshot)
    
    strGeoID = rs("GRPgeoID")
    'strAgentID1 = rs("GRagentID1")
    'strAgentID2 = rs("GRagentID2")
    
    'agent names
    'strSQL = "SELECT tblAgent.AGENTid, tblAgent.AGENT_LUname FROM tblAgent WHERE (((tblAgent.AGENTid)= '" & strAgentID1 & "'));"
    'Set rs = db.OpenRecordset(strSQL, dbOpenSnapshot)
    'Forms![frmInvGrp_Bexar]![AGENT1 NAME] = rs("AGENT_LUname")
    
    'strSQL = "SELECT tblAgent.AGENTid, tblAgent.AGENT_LUname FROM tblAgent WHERE (((tblAgent.AGENTid)= '" & strAgentID2 & "'));"
    'Set rs = db.OpenRecordset(strSQL, dbOpenSnapshot)
    'Forms![frmInvGrp_Bexar]![AGENT2 NAME] = rs("AGENT_LUname")
    
    'geo id
    strSQL = "SELECT tblGeographical.GEOid, tblGeographical.GEOdesc " & _
    "FROM tblGeographical " & _
    "WHERE (((tblGeographical.GEOid)='" & strGeoID & "'));"
    
    Set rs = db.OpenRecordset(strSQL, dbOpenSnapshot)
    Forms![frmInvGrp_Bexar]![TERRITORY NAME] = rs("GEOdesc")
    
    rs.Close
    db.Close
    Set db = Nothing
    
End Sub

Private Sub Form_Load()

On Error GoTo Err_Form_Load
  
    Dim db As DAO.Database
    Dim rs As DAO.Recordset
    Dim strWhere As String
    Dim strCriterion As String
    Dim strTranNo As String
    Dim lngSysDocID As Long
    Dim varRecTime As Variant
    Dim varRecDate As Variant
    Dim strRecUserID As String
    Dim dteInvoiceDate As Date
    Dim strSQL As String
    
    'check to see if there are any invoices in tblInvHdr
    'if there are invoices in the table,
    'then exit sub
    Set db = CurrentDb()
    strSQL = "SELECT tblInvHdr.[CUST KEY]FROM tblInvHdr;"
    Set rs = db.OpenRecordset(strSQL, dbOpenSnapshot)
    If rs.RecordCount > 0 Then
        rs.Close
        db.Close
        Set db = Nothing
        MsgBox "There are other invoices waiting to be posted to Platinum."
        Exit Sub
    End If
    
    DoCmd.Hourglass True
    
    'get the current invoice from tblConstansts
    strSQL = "SELECT tblConstants.FieldName, tblConstants." & _
             "FieldDateValue " & _
             "FROM tblConstants WHERE tblConstants.FieldName = 'InvoiceDate';"

    Set rs = db.OpenRecordset(strSQL, dbOpenSnapshot)
    dteInvoiceDate = rs.Fields("FieldDateValue")
    
    'Get the next Platinum tran no from ARONE_R9
    strTranNo = GetCurrentTranNo()
    If strTranNo = "0" Then
        GoTo Exit_Form_Load
    Else
        Forms![frmInvGrp_Bexar]![TRANS NO] = Mid(Year(dteInvoiceDate), 4, 1) & Format(strTranNo, "000000")
    End If
    
    'Get the next SysDocID from ARNEXTSY
    lngSysDocID = GetCurrentSysDocID()
    
    'populate form with data
    Forms![frmInvGrp_Bexar]![SYSDOCID] = lngSysDocID
    Forms![frmInvGrp_Bexar]![RecUserID] = "INVOICE"
    Forms![frmInvGrp_Bexar]![RecDate] = date
    Forms![frmInvGrp_Bexar]![RecTime] = Time()
    
    Set db = CurrentDb()
    Set rs = db.OpenRecordset("tblGrp_Bexar", dbOpenSnapshot)
    Forms![frmInvGrp_Bexar]![CUST KEY] = rs("GroupID")
    Forms![frmInvGrp_Bexar]![CUST NAME] = rs("GRname")
    Forms![frmInvGrp_Bexar]![ADDRESS 1] = rs("GRstreet1")
    Forms![frmInvGrp_Bexar]![ADDRESS 2] = rs("GRstreet2")
    Forms![frmInvGrp_Bexar]![CITY] = rs("GRcity")
    Forms![frmInvGrp_Bexar]![STATE] = rs("GRstate")
    Forms![frmInvGrp_Bexar]![ZIP CODE] = rs("GRzip")
    Forms![frmInvGrp_Bexar]![ATTENTION] = rs("GRmainCONT")
    Forms![frmInvGrp_Bexar]![TERRITORY KEY] = rs("GRPgeoID")
    Forms![frmInvGrp_Bexar]![AGENT1] = rs("GRagentID1")
    Forms![frmInvGrp_Bexar]![AGENT1 RATE] = rs("GRagentRATE1")
    Forms![frmInvGrp_Bexar]![AGENT2] = rs("GRagentID2")
    Forms![frmInvGrp_Bexar]![AGENT2 RATE] = rs("GRagentRATE2")
    
    'invoice date
    strSQL = "SELECT tblConstants.FieldName, tblConstants.FieldDateValue FROM tblConstants;"
    Set rs = db.OpenRecordset(strSQL, dbOpenSnapshot)
    Forms![frmInvGrp_Bexar]![INV DATE] = rs("FieldDateValue")
    
    'save everything
    SendKeys "+{ENTER}", True
    
    DoCmd.Hourglass False
    
    rs.Close
    db.Close
    Set db = Nothing
    
Exit_Form_Load:
    DoCmd.Hourglass False
    Exit Sub

Err_Form_Load:
  MsgBox "The following error occured: " & Error$
  GoTo Exit_Form_Load
End Sub