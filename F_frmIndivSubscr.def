Function CreateAgentRecord(strAgentID As String, PlanID As Integer, CoverID As Integer) As Boolean
    On Error GoTo PROC_ERR
    
    Dim strSQL As String
    strSQL = "insert into tblIndivAgt " & _
                "(SubscrID, PltCustKey, AgentID, [Primary], AgentRate) " & _
                "values('" & Me.SubID & "', '" & Me.PLTcustKEY & _
                "', '" & strAgentID & "', " & 1 & ", " & 0 & ");"

    ExecuteSQL strSQL
    
    CreateAgentRecord = True
    
PROC_EXIT:
  DoCmd.SetWarnings True
  DoCmd.Hourglass False
  Exit Function
    
    
PROC_ERR:
  MsgBox "Error: (" & Err.Number & ") " & Err.DESCRIPTION, vbCritical, "Function:  Form_frmIndivSubscr.CreateAgentRecord"
  Resume PROC_EXIT

End Function