Private Sub txtEmailAddress_BeforeUpdate(Cancel As Integer)
     
    On Error GoTo Err_txtEmailAddress_BeforeUpdate
    
    Dim DotEnding As String
    Dim InputErr As Integer
    
    If Not IsNull(Me.txtEmailAddress) Or Me.txtEmailAddress <> "" Then
        DotEnding = Right(txtEmailAddress, 4)
        InputErr = 0
    End If
    
    If Not IsNull(Me.txtEmailAddress) Or Me.txtEmailAddress <> "" Then
        If InStr(Me.txtEmailAddress, " ") > 0 Then
            InputErr = InputErr + 1
        End If
    End If

    If Not IsNull(Me.txtEmailAddress) Or Me.txtEmailAddress <> "" Then
        If Len(Me.txtEmailAddress) - Len(Replace(Me.txtEmailAddress, "@", "")) <> 1 Then
            InputErr = InputErr + 1
        End If
    End If
    
    If Not IsNull(Me.txtEmailAddress) Or Me.txtEmailAddress <> "" Then
        If DotEnding <> ".com" And DotEnding <> ".org" And DotEnding <> ".edu" And DotEnding <> ".net" Then
            InputErr = InputErr + 1
        End If
    End If

    
    If InputErr > 0 Then
        MsgBox "You Have Entered An Invalid Email Address!", vbInformation, "QCD: Individual"
        Cancel = True
        Me.txtEmailAddress.SetFocus
    End If
    
    
    Exit Sub

Exit_txtEmailAddress_BeforeUpdate:
    Exit Sub

Err_txtEmailAddress_BeforeUpdate:
    If Err = 94 Then  'invalid use of null
        Resume Next
    End If
    
    MsgBox "Error number " & Err.Number & ": " & Err.DESCRIPTION
    GoTo Exit_txtEmailAddress_BeforeUpdate

End Sub

