Private Function Run_Procedure(strSQL As String, strSubID As String, strUserName As String) As Boolean

On Error GoTo PROC_ERR
    
    If g_adoConnection.STATE = adStateOpen Then g_adoConnection.Close
    With g_adoConnection
         .ConnectionString = SetConnxn
         .CommandTimeout = 300
         .Open
    End With
    
    With g_adoCommand
          .ActiveConnection = g_adoConnection
          .CommandTimeout = 300
          .CommandType = adCmdStoredProc
          .CommandText = strSQL
          
          Set g_adoParm = .CreateParameter("Return", adInteger, adParamReturnValue) 'Return parm
          .Parameters.Append g_adoParm
          
          Set g_adoParm = .CreateParameter("@SubID", adChar, adParamInput, 8)
          .Parameters.Append g_adoParm
          .Parameters("@SubID").value = strSubID
          
          Set g_adoParm = .CreateParameter("@UserName", adVarChar, adParamInput, 20)
          .Parameters.Append g_adoParm
          .Parameters("@UserName").value = strUserName
          
          .Execute
    End With
    
    g_iRtnVal = g_adoCommand(0)
    
    If g_iRtnVal = 1 Then
        Run_Procedure = True
    Else
        Run_Procedure = False
    End If
    
    g_adoConnection.Close
  
    Set g_adoCommand = Nothing
    Set g_adoConnection = Nothing
    Set g_adoParm = Nothing
  
    
PROC_EXIT:
  DoCmd.SetWarnings True
  DoCmd.Hourglass False
  Exit Function
    
    
PROC_ERR:
  MsgBox "Error: (" & Err.Number & ") " & Err.DESCRIPTION, vbCritical, "Function:  Form_frmIndivSubscr.Run_Procedure"
  Resume PROC_EXIT
    
End Function

'   ---------   btnDelete_Click    -----------
Public Sub btnDelete_Click()

On Error GoTo Err_btnDelete_Click
    
    Dim strSubID As String
    Dim strUserName As String
    Dim intAns As Integer
    
    intAns = MsgBox("Are you sure you want to permanently delete this subscriber?", vbExclamation + vbYesNo, "Confirm Delete")
    If intAns = vbNo Then
        GoTo Exit_btnDelete_Click
    End If
    
    strSubID = Me![SubID]
    strUserName = GetCurrentUserName()

    DoCmd.Hourglass True
    
    Me.Dirty = False ' save the record
    
    If DeleteARCust(strSubID, strUserName) Then
            MsgBox "Customer was deleted from ARCUST_local!"
    Else
            MsgBox "Customer was not deleted from ARCUST_local " & vbCrLf & "because the record was not in ARCUST_local.", vbInformation, "QCD Indiv: Delete Status"
    End If
    
    If DeleteIndividualSubscriber(strSubID, strUserName) Then
            MsgBox "Customer was deleted from tblSubscr!"
    Else
            MsgBox "Customer was not deleted from tblSubscr " & vbCrLf & "because the record was not in tblSubscr.", vbInformation, "QCD Indiv: Delete Status"
    End If
    
    DoCmd.Echo False
    DoCmd.Hourglass False
    DoCmd.SetWarnings True
    
    Me.Requery
    
    Exit Sub
    
Exit_btnDelete_Click:
    DoCmd.Echo False
    DoCmd.Hourglass False
    DoCmd.SetWarnings True
    Exit Sub

Err_btnDelete_Click:
    If Err = 2501 Then  ' the action was cancelled
        Resume Next
    Else
        MsgBox "Error number " & Err.Number & ": " & Err.DESCRIPTION
        Resume Exit_btnDelete_Click
    End If
End Sub
'----   end btnDelete_Click   ------

Private Sub cboGroupID_AfterUpdate()
'------------------------------------------------------------
'  Description:  This is the individual billing group control
'               i.e. Individual-Monthly, Individual-One Year,
'               Individual-Two Year, etc..
'               Individuals differ from group subscribers
'               in that they do not always pay the "standard"
'               rate.  Often times, they will negotiate an
'               actual rate which is lower.  The standard rate
'               is in tblRates and joined to tblSubscr through
'               the RateID or GroupID|PlanID|CoverID.  The
'               actual rate is on tblSubscr.
'------------------------------------------------------------
    On Error GoTo PROC_ERR
        
    '  populate standard rate control
    '  from the selected Billing Group
    '  from the combo box
    Dim strGroupID As String
    Dim intCoverID As Integer
    Dim intPlanID As Integer

    With Me
        strGroupID = .cboGroupID.Column(2)
        intCoverID = .cboGroupID.Column(3)
        intPlanID = 5                                               '  All Individuals are on 5 (QCD Only)
        .SubStdRate = FindRate(strGroupID, intCoverID, intPlanID)   '  populate the Standard Rate control
        .RateID = GetRateID(strGroupID, intPlanID, intCoverID)      '  populate the RateID control (hidden)
    End With
    
Exit Sub

PROC_EXIT:
  DoCmd.SetWarnings True
  DoCmd.Hourglass False
  Exit Sub

PROC_ERR:
  MsgBox "Error: (" & Err.Number & ") " & Err.DESCRIPTION, vbCritical, "Procedure: frmIndivSubscr.cboGroupID_AfterUpdate"
  Resume PROC_EXIT
End Sub
'---------  end of cboGroupID_AfterUpdate   ------------

