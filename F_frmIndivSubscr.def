Private Sub Form_BeforeUpdate(Cancel As Integer)
'------------------------------------------------
'Purpose:  validates controls on the form
'
'------------------------------------------------
'Programmer:  John Criswell
'Create Date:
'Update Date:
'Updated by:  John Criswell

On Error GoTo Err_Form_BeforeUpdate
    
    Dim strSSN As String
    Dim intDepCnt As Integer
    Dim intCoverID As Integer
    
    strSSN = Me.SubSSN
    intDepCnt = CountDependents(strSSN)

    ' set the coverage based on
    ' the number of dependents
    If intDepCnt = 0 Then
        intCoverID = 1  ' individual only
    ElseIf intDepCnt = 1 Then
        intCoverID = 2  ' individual plus wife or child
    ElseIf intDepCnt > 1 Then
        intCoverID = 4  ' individual plus wife and child(ren)
    End If
    
    ' for existing records only
    If Not mfNewRecord Then
       ' applies only to active individuals
        If Me.txtCancelled = 1 Then
            ' cboSubCoverID ?
            If Me.cboSubCoverID <> intCoverID Then
                MsgBox "The coverage does not match the number of dependents.  Please correct", vbInformation, "QCD:  Individual"
                Cancel = True
                Me.cboSubCoverID.SetFocus
                GoTo Exit_Form_BeforeUpdate
            End If
        
            ' SubContBeg ?
            If IsNull(Me.SubContBeg) Then
                MsgBox "Please enter a contract begin date.", vbInformation, "QCD:  Individual"
                Cancel = True
                Me.SubContBeg.SetFocus
                GoTo Exit_Form_BeforeUpdate
            End If
        
            ' SubContEnd ?
            If IsNull(Me.SubContEnd) Then
                MsgBox "Please enter a contract end date.", vbInformation, "QCD:  Individual"
                Cancel = True
                Me.SubContEnd.SetFocus
                GoTo Exit_Form_BeforeUpdate
            End If
        
            ' SubContEnd ?
            If Me.SubContEnd <= Me.SubContBeg Then
                MsgBox "The contract end date cannot be before the begin date.", vbInformation, "QCD:  Individual"
                Cancel = True
                Me.SubContEnd.SetFocus
                GoTo Exit_Form_BeforeUpdate
            End If
        
             ' SubDOB ?
            If IsNull(Me.SUBdob) Then
                MsgBox "Please enter a date of birth.", vbInformation, "QCD:  Individual"
                Cancel = True
                Me.SUBdob.SetFocus
                GoTo Exit_Form_BeforeUpdate
            End If
            
              ' RateID ?
            If IsNull(Me.RateID) Then
                MsgBox "Please enter an actual rate.", vbInformation, "QCD:  Individual"
                Cancel = True
                Me.RateID.SetFocus
                GoTo Exit_Form_BeforeUpdate
            End If
        End If
    End If
    
Exit_Form_BeforeUpdate:
    DoCmd.Hourglass False
    Exit Sub

Err_Form_BeforeUpdate:
    If Err = 94 Then
        Resume Next
    Else
        MsgBox Error$
        GoTo Exit_Form_BeforeUpdate
    End If
End Sub



Private Sub Form_Current()
    
    On Error GoTo Err_Form_Current
    
    With Me
        If .txtCancelled = 2 Then
            ' On Hold yellow
            .SubFirstName.BackColor = vbYellow
            .SubLastName.BackColor = vbYellow
            .SubMiddleName.BackColor = vbYellow
            .txtCancelled.BackColor = vbYellow
            ElseIf .txtCancelled = 3 Then
            ' Cancelled to Red
            .SubFirstName.BackColor = vbRed
            .SubLastName.BackColor = vbRed
            .SubMiddleName.BackColor = vbRed
            .txtCancelled.BackColor = vbRed
        Else
            ' Active green
            .SubFirstName.BackColor = vbGreen
            .SubLastName.BackColor = vbGreen
            .SubMiddleName.BackColor = vbGreen
            .txtCancelled.BackColor = vbGreen
        End If
        .Repaint
    End With
    
    With Me
        If Not IsNull(.SubSSN) Then
            .[AlphaControl] = Null
            .[ClientPickList] = Null
            .FindSSN = Null
            .FindSUBID = Null
        End If
    End With
    
    'set the new record variable
    mfNewRecord = Me.NewRecord
    
    
    'if this is not a new record
    'then populate the std rate control
    If mfNewRecord = False Then
        Call fsubIndivSubscrDep_Exit(0)
    End If
    
    Me.SubLastName.SetFocus
    
    
Exit_Form_Current:
    Exit Sub
    
Err_Form_Current:
    MsgBox Error$
    Resume Exit_Form_Current
    
End Sub

