Private Sub Form_AfterUpdate()

    '------------------------------------------------
    'Purpose:  updates ARCUST_local with changes
    '
    '------------------------------------------------
    'Programmer:    John Criswell
    'Create Date:   2014-10-16
    'Update Date:
    'Updated by:    John Criswell
    
    On Error GoTo PROC_ERR
    
    Dim strSQL As String
    Dim strUserName As String
    Dim strSubID As String
        
    DoCmd.Hourglass True
    DoCmd.SetWarnings False

    ' sync the customer information to ARCUST_local
    strSQL = "usp_CustIndivRefresh"
    strSubID = Me.SubID
    strUserName = GetCurrentUserName()
    If Run_Procedure(strSQL, strSubID, strUserName) Then
        GoTo PROC_EXIT
    Else
        Err.Raise vbObjectError + 1001, , "ARCUST_local was not updated.  Contact support"
        GoTo PROC_ERR
    End If
    
        
PROC_EXIT:
    DoCmd.SetWarnings True
    DoCmd.Hourglass False
    Exit Sub
    
PROC_ERR:
  MsgBox "Error: (" & Err.Number & ") " & Err.DESCRIPTION, vbCritical, "Procedure:  frmIndivSubscr.Form_AfterUpdate"
  Resume PROC_EXIT
  
End Sub

