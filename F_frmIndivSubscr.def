Private Sub fsubIndivSubscrDep_Exit(Cancel As Integer)
    On Error GoTo fsubIndivSubscrDep_Exit_Err
    '-----------------------------------------------------------
    'Description:  This happens when the user exits the dependents
    '              subform.  It will update the Rate and RateID
    '              controls based on the number of dependents
    '              the user entered on the subform
    '
    '-----------------------------------------------------------
    
    Dim intDepCount As Integer
    Dim strSSN As String
    Dim strSQL
    Dim strGroupID As String
    Dim intPlanID As Integer
    Dim intCoverID As Integer
    
    
    '  if this is a new record then skip coverage rate update
    If mfNewRecord Then
        GoTo fsubIndivSubscrDep_Exit_Exit
    Else
        If Me.SubSSN <> "" Then
            strSSN = Me.SubSSN
            
            ' populate the dependent count control
            ' on the main form
            ' from the number of dependents entered
            intDepCount = CountDependents(strSSN)
            If Forms![frmIndivSubscr]![DepCnt] <> intDepCount Then
                Forms![frmIndivSubscr]![DepCnt] = intDepCount
            End If
            
            ' set the coverage based on
            ' the number of dependents
            Select Case intDepCount
                Case 0
                intCoverID = 1 ' individual only
                Case 1
                intCoverID = 2 ' individual plus wife or child
                Case Else
                intCoverID = 4 ' individual plus wife and child(ren)
            End Select
            Me.cboSubCoverID = intCoverID
            With Me
                ' populate the rate controls
                '   the hidden control for RateID - RateID
                '   the actual rate control - SubRate
                '   the standard rate control - SubStdRate
                .RateID = GetRateID(.cboGroupID, .PlanID, .CoverID)
                .SubStdRate = FindRate(.cboGroupID, .CoverID, .PlanID)
            End With
        End If
    End If
    
    
    Exit Sub
    
fsubIndivSubscrDep_Exit_Exit:
    Exit Sub
    
fsubIndivSubscrDep_Exit_Err:
    ' Display error information.
    MsgBox "Error number " & Err.Number & ": " & Err.Description
    GoTo fsubIndivSubscrDep_Exit_Exit
    
End Sub



