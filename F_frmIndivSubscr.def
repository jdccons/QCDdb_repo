Private Sub SUBssn_AfterUpdate()

On Error GoTo PROC_ERR

    Dim strAgentID As String
    Dim strSubSSN As String
    
    If mfNewRecord Then
        With Me
            strAgentID = "CORPT"
            strSubSSN = .SubSSN
            .EIMBRID = .SubSSN & "00"
            .PlanID = 5
            
            ' cover group
            If .cboGroupID <> "INDV1" Then
                ' nothing
            Else
                .cboGroupID = "INDV1"
            End If
            
            ' set the coverage based on count of dependents
            .DepCnt = CountDependents(strSubSSN)
            If .DepCnt = 0 Then
                .cboSubCoverID = 1
            ElseIf .DepCnt = 1 Then
                .cboSubCoverID = 2
            Else
                .cboSubCoverID = 4
            End If
           
            .SubStatus = "INDIV"
            .SubClassKey = "INDIV"
            
            ' populate the rate controls
                '   the hidden control for RateID - RateID
                '   the actual rate control - SubRate
                '   the standard rate control - SubStdRate
                .RateID = GetRateID(.cboGroupID, .PlanID, .CoverID)
                .SubStdRate = FindRate(.cboGroupID, .CoverID, .PlanID)
            
            ' save the user who created this transaction
            .UserName = GetCurrentUserName()
            
            ' save the date and the transaction type
            .DateCreated = date
            .TransactionType = "ADDED"
            
            .User01 = "frmIndivSubscr"
            .User02 = "create new individual"
            .User04 = date
            
            ' save the record
            Me.Dirty = False
            
            ' no longer a new record
            mfNewRecord = False
            
            ' refresh the find combo box
            .ClientPickList.Requery
            
            ' create an agent record in tblIndivAgt
            If CreateAgentRecord(strAgentID, .PlanID, .CoverID) Then
                ' requery the agent subform
                Forms!frmIndivSubscr![sfrmInvAgents].Requery
            End If
        End With
    End If
    
PROC_EXIT:
  DoCmd.SetWarnings True
  DoCmd.Hourglass False
  Exit Sub

PROC_ERR:
  MsgBox "Error: (" & Err.Number & ") " & Err.Description, vbCritical, "Procedure: frmIndivSubscr.SubSSN_AfterUpdate"
  Resume PROC_EXIT
End Sub

