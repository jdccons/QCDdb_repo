Private Sub btnDelete_Click()

    On Error GoTo PROC_ERR
    
    Dim strSQL As String
    Dim UserName As String
    
    UserName = GetCurrentUserName()
    DoCmd.RunCommand acCmdSaveRecord
    strSQL = "exec usp_UpdateDeleted" & " NULL" & ",' " & UserName & "', ''"
    ExecuteSQL strSQL
    DoCmd.Requery
    Me.Refresh
    
PROC_EXIT:
    Exit Sub
    
PROC_ERR:
    MsgBox "Error: (" & Err.Number & ") " & Err.Description, vbCritical, "Procedure: frmPendingDeletions.btnDelete"
    GoTo PROC_EXIT
End Sub
    


