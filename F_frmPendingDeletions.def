Private Sub btnDelete_Click()

    On Error GoTo PROC_ERR
    
    Dim strSQL As String
    Dim UserName As String
    
    UserName = GetCurrentUserName()
    DoCmd.RunCommand acCmdSaveRecord
    strSQL = "exec usp_UpdateDeleted" & " NULL" & ",' " & UserName & "', ''"
    ExecuteSQL strSQL
    DoCmd.Requery
    Me.Refresh
    
PROC_EXIT:
    Exit Sub
    
PROC_ERR:
    MsgBox "Error: (" & Err.Number & ") " & Err.DESCRIPTION, vbCritical, "Procedure: frmPendingDeletions.btnDelete"
    GoTo PROC_EXIT
End Sub
    


Private Sub btnDeletePick_Click()
    
    On Error GoTo PROC_ERR
    
    Dim DateDeleted As Date
    Dim sDateDeleted As String
    Dim strSQL As String
    Dim UserName As String
    
    UserName = GetCurrentUserName()
    DateDeleted = DeleteDateCalendar.Value
    sDateDeleted = CStr(Year(DateDeleted)) & "-" & IIf(Len(CStr(Month(DateDeleted))) = 1, "0" & CStr(Month(DateDeleted)), CStr(Month(DateDeleted))) & "-" & IIf(Len(CStr(Day(DateDeleted))) = 1, "0" & CStr(Day(DateDeleted)), CStr(Day(DateDeleted))) & " 00:00:00"
    sDateDeleted = "'" & sDateDeleted & "'"
    sDateDeleted = sDateDeleted & ", '" & UserName & "', ''"
    DoCmd.RunCommand acCmdSaveRecord
    strSQL = "exec usp_UpdateDeleted " & sDateDeleted
    ExecuteSQL strSQL
    DoCmd.Requery
    Me.Refresh
    
PROC_EXIT:
