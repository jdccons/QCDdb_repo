Private Sub Report_Activate()
    If Me.GroupType = 4 Then
       Me.srptGrpAA_Cnts.Visible = True
       Me.srptGrpFOC_Cnts.Visible = False
    ElseIf Me.GroupType = 2 Or Me.GroupType = 3 Then
       Me.srptGrpAA_Cnts.Visible = False
       Me.srptGrpFOC_Cnts.Visible = True
    Else
       Me.srptGrpAA_Cnts.Visible = False
       Me.srptGrpFOC_Cnts.Visible = False
    End If
End Sub
Private Sub Report_Close()

  On Error GoTo PROC_ERR
    
    DoCmd.Restore
    Forms![fdlgPrtInv_AA].Visible = False
    DoCmd.Close A_FORM, "fdlgPrtInv_AA"
    Forms![fmnuSwitchboard].Visible = True

  Exit Sub

PROC_ERR:
  MsgBox "The following error occured: " & Error$
  Resume Next
End Sub