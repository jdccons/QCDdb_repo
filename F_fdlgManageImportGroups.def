        .txtImportName = .cboImportSpec.Column(1)
    End With
    
End Sub
Private Sub cmdAddSpec_Click()
    'Purpose:  This procedure adds a file spec to the lookup table
    '          tlkpImportSpec
    '--------------------------------------------------------
    ' Parameters:  - None
    ' Programmer:  John Criswell
    ' Create Date:  12/07/07
    ' Update Date:  12/07/07
    ' Updated by:  John Criswell
    ' Reason:
    '---------------------------------------------------------
        
    On Error GoTo ErrorHandler
        
    Dim db As DAO.Database
    Dim strSQL As String
    
    Set db = CurrentDb()
    
    DoCmd.SetWarnings False
    DoCmd.Hourglass True
    With Me
        If Not IsNull(.txtGROUPid) Or Not IsNull(.txtImportID) Or Not IsNull(.txtImportName) Then
            strSQL = "Insert INTO tlkpImportSpec (GroupID, ImportType, ImportDesc) " & _
            "VALUES ('" & .txtGROUPid & "'," & .txtImportID & ",'" & .txtImportName & "')"
            db.Execute strSQL, dbSeeChanges
        Else
            MsgBox "You must populate the combo boxes!"
        End If
    End With
    DoCmd.Hourglass False
    DoCmd.SetWarnings True
    
    Set db = Nothing
        
Exit_Point:
    Exit Sub
    
ErrorHandler:
    MsgBox Err.DESCRIPTION & " " & Err.Number
    Resume Exit_Point
End Sub


Private Sub cmdClose_Click()
On Error GoTo Err_cmdClose_Click


    DoCmd.Close

Exit_cmdClose_Click:
    Exit Sub

Err_cmdClose_Click:
    MsgBox Err.DESCRIPTION
    Resume Exit_cmdClose_Click
    
End Sub