    On Error GoTo PROC_ERR
    
    With Me
        .txtMainFile = ""
        .txtDependentFile = ""
        .txtStatus = ""
        .txtStatusDataImport = ""
    End With
    
PROC_EXIT:
    Exit Sub

PROC_ERR:
    MsgBox Err.DESCRIPTION
    Resume PROC_EXIT
    

End Sub