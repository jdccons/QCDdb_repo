Private Sub CUST_KEY_AfterUpdate()

'Purpose:  gets information for the form based on the customer key,
'          and appends data to tblInvHdr
'Programmer:  Suzanne Johnsen
'Date Created:  9/21/99
'Date Updated:
'Updated by:

  On Error GoTo PROC_ERR
    'Check to see if this is an existing record
    'or a new one.  It is an existing record
    'if the tran no control has a value in it.
    If Not IsNull(Me![TRANS NO]) Then
        MsgBox ("This is not a new record")
        Exit Sub
    End If

    Dim db As DAO.Database
    Dim rs As DAO.Recordset
    Dim strTranNo As String
    Dim lngSysDocID As Long
    Dim varInvoiceDate As Variant
    Dim strSQL As String
    Dim strCustomerKey As String
    
    'DoCmd.Hourglass True
    'DoCmd.SetWarnings False
    
    Set db = CurrentDb()
    
    'get the subscriber id based upon the platinum customer key for the individual
    strCustomerKey = Forms!frmInvIndiv![CUST KEY]
    strSQL = "SELECT tblSubscr.PLTcustKEY, tblInvAgt.AGENTid, " & _
             "tblInvAgt.SubscrID " & _
             "FROM tblSubscr INNER JOIN tblInvAgt ON tblSubscr." & _
             "SubID = tblInvAgt.SubscrID " & _
             "WHERE (((tblSubscr.PLTcustKEY)='" & strCustomerKey & "'));"
    Set rs = db.OpenRecordset(strSQL, dbOpenSnapshot)
    Forms![frmInvIndiv]![SubID] = rs.Fields("SubscrID")
    rs.Close
    
    'get the invoice date from tblConstants
    Set rs = db.OpenRecordset("tblConstants", dbOpenSnapshot)
    varInvoiceDate = rs("FieldDateValue")
    rs.Close

    'Get the current Platinum tran no
    strTranNo = GetCurrentTranNo()
    Forms![frmInvIndiv]![TRANS NO] = Mid(Year(varInvoiceDate), 4, 1) & Format(strTranNo, "000000")
        
    'Get the current SysDocID from ARNEXTSY
    lngSysDocID = GetCurrentSysDocID()
       
    With Forms![frmInvIndiv]
        [SYSDOCID] = lngSysDocID
        [RecUserID] = "INVOICE"
        [RecDate] = date
        [RecTime] = Time()
    End With

    strSQL = "SELECT tblSubscr.SUB_LUname, tblSubscr.SUBstreet1," & _
             " tblSubscr.SUBstreet2, tblSubscr.SUBcity, " & _
             "tblSubscr.SUBstate, tblSubscr.SUBzip, tblSubscr." & _
             "SUBgeoID " & _
             "FROM tblSubscr " & _
             "WHERE (((tblSubscr.PLTcustKEY)='" & strCustomerKey & "'));"
    
    Set rs = db.OpenRecordset(strSQL, dbOpenSnapshot)
    With Forms![frmInvIndiv]
        [CUST NAME] = rs("SUB_LUNAME")
        [ADDRESS 1] = rs("SUBstreet1")
        [ADDRESS 2] = rs("SUBstreet2")
        [CITY] = rs("SUBcity")
        [STATE] = rs("SUBstate")
        [ZIP CODE] = rs("SUBzip")
        [TERRITORY KEY] = rs("SUBgeoID")
        [INV DATE] = varInvoiceDate
    End With
    
    
    'find agents for individual subscriber
    'strSubID = rs("SubID")
    'intCnt = 0
    'strSQL = "SELECT tblInvAgt.SubscrID, tblInvAgt.AGENTid, tblInvAgt.AgentRATE, tblInvAgt.Primary, tblInvAgt.Sort " & _
    '         "FROM tblInvAgt WHERE (((tblInvAgt.SubscrID)='" & strSubID & "')) ORDER BY tblInvAgt.Sort;"
    'Set rs = db.OpenRecordset(strSQL, dbOpenSnapshot)
    'If rs.RecordCount > 0 Then
    ' Do While Not rs.EOF
    '    If rs.Fields("Primary") = True Then
    '        strAgent1 = rs("AGENTid")
    '        dblAgt1Comm = rs("AgentRATE")
    '    ElseIf intCnt = 1 Then
    '        strAgent2 = rs("AGENTid")
    '        dblAgt2Comm = rs("AgentRATE")
    '    Else
    '
    '    End If
    '    intCnt = intCnt + 1
    '    rs.MoveNext
    ' Loop
    'End If
    
    rs.Close
    db.Close
    Set db = Nothing

    SendKeys "+{ENTER}", True
    'DoCmd.SetWarnings True
    'DoCmd.Hourglass False

  Exit Sub

PROC_ERR:
  MsgBox "The following error occured: " & Error$
  Resume Next
End Sub