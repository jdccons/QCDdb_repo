' --------------   run stored procedure to create csv file   ---------


' --------------   create individual invoices   ---------
Public Sub btnBulkInvoices_Click()
    
    '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
    ' Object:               btnBulkInvoices_Click(
    ' Purpose:              This procedure creates multiple invoices;
    '                       calls a stored procedure that retrieves data
    '                       from the SQL Server database and populates a
    '                       temp table bound to the form
    '
    ' Stored Procedure:     usp_InvoicePreIndivAll
    ' Parameters:           InvoiceDate DateTime
    '                       UserName nvarchar(20)
    '
    '
    ' Date Created:         9/15/2014
    ' Developer:            John Criswell
    '
    ' Change Log
    ' Date Modified     Developer       Purpose
    ' ------------------------------------------------------------
    ' 2014-09-14        J Criswell      Created
    '
    ' VBA Procedure Parameters:
    ' ------------------------------------------------------------
    ' None
    '
    ' Returns:
    ' ------------------------------------------------------------
    ' Nothing
    '
    '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
    
    On Error GoTo PROC_ERR
    
    Dim dteInvoiceDate As Date
    Dim strUserName As String
    Dim strCustKey As String
    Dim proc_name As String
    
    dteInvoiceDate = Me.INPUT_DATE
    ' grab the user's name for parameter to stored procedure
    strUserName = GetCurrentUserName()
    strCustKey = ""
    proc_name = "usp_InvoicePreIndivAll"
    
    
    DoCmd.Hourglass True
    DoCmd.SetWarnings False
    
    If IsNull(Me.INPUT_DATE) Or Me.INPUT_DATE = "" Then
        MsgBox "Input date required."
        Me.INPUT_DATE.SetFocus
    Else
        
        ' check for duplicate PltCustKeys
        If DupPltCustKeys() Then
            MsgBox "There are duplicate PltCustKeys.  The process will fail unless the duplicates are fixed."
            GoTo PROC_EXIT
        Else
            ' save the date entered in INPUT_DATE
            Call SaveInvoiceDate(dteInvoiceDate)
            
            ' create the invoice
            If Create_Invoice_Data(proc_name, dteInvoiceDate, strCustKey, strUserName) Then
                MsgBox "The invoices were created and stored in a temp table.", vbOKOnly, "QCD: Create One Off Invoices"
                Me.[INPUT_DATE] = ""
                Forms![frmInvIndiv].Requery
            Else
                MsgBox "There was an issue creating your invoice. Contact support.", vbOKOnly, "QCD: Create One Off Invoices"
                Me.Refresh
            End If
        End If
    End If
    
    DoCmd.Hourglass False
    DoCmd.SetWarnings True
    
PROC_EXIT:
    DoCmd.SetWarnings True
    DoCmd.Hourglass False
    Exit Sub
    
PROC_ERR:
    If Err.Number = 94 Then
        Resume Next
    Else
        MsgBox "The following error occured: " & Error$
        Resume PROC_EXIT
    End If
    
End Sub
'frmInvIndiv_btnPostAlltoPlat_Click
' --------------   event to call stored procedure to save invoice(s) to a csv file   ---------
Public Sub btnPostAlltoPlat_Click()
    
    '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
    ' Object:               btnPostAlltoPlat_Click
    ' Purpose:              Post sales receipts transactions to
    '                       transaction table and exports transactions
    '                       to a csv file.
    '
    '
    '
    '
    ' Stored Procedure:
    ' Parameters:
    '
    '
    '
    ' Date Created:         9/15/2014
    ' Developer:            John Criswell
    '
    ' Change Log
    ' Date Modified     Version Developer       Purpose
    ' ------------------------------------------------------------
    ' 2014-09-14                J Criswell      Created
    '
    ' VBA Procedure Parameters:
    ' ------------------------------------------------------------
    ' None
    '
    ' Returns:
    ' ------------------------------------------------------------
    ' Nothing
    '
    '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
    
    On Error GoTo PROC_ERR
        
    Dim strGroupType As String
    Dim dteInvoiceDate As Date
    Dim strUserName As String
    Dim strInput As String
    
    strGroupType = "Individual"
    strUserName = GetCurrentUserName()
    strInput = InputBox("Enter an invoice date.")
    Select Case Len(strInput)
        Case 0
            GoTo PROC_EXIT
        Case Else
            Do While Not IsDate(strInput)
                Select Case Len(strInput)
                    Case 0
                        GoTo PROC_EXIT
                    Case Else
                        strInput = InputBox("The date you entered is not a valid date." & vbCrLf & "Please re-enter a valid invoice date")
                End Select
            Loop
    End Select
    dteInvoiceDate = CDate(strInput)
    
    DoCmd.Hourglass True
    DoCmd.SetWarnings False
    
    '  duplicate PltCustKeys?
    If DupPltCustKeys() Then
        MsgBox "There are duplicate PltCustKeys.  The process will fail unless the duplicates are fixed.", vbOKOnly, "QCD: Post Monthlies"
        GoTo PROC_EXIT
    End If
    
    ' duplicate invoices?
    If AreAnyInvoicesInPlat(dteInvoiceDate, strGroupType) Then
        MsgBox "One or more of the invoices already exist in Platinum.  Please remove from Platinum and start over.", vbOKOnly, "QCD: Post Monthlies"
        GoTo PROC_EXIT
    End If
    
    ' sync ARCUST_local
    If SyncCustomers("usp_CustIndivRefreshAll", strUserName) Then
        ' nothing
    Else
        MsgBox "Customer sync failed.  Contact support.", vbOKOnly, "QCD: Post Monthlies"
        GoTo PROC_EXIT
    End If
    
    ' post invoices
    ' in pre-condition to
    ' ARTRANH_local
    If Export_Invoice_Data("usp_InvoicePostIndivAll", dteInvoiceDate, strUserName) Then
        MsgBox "Invoices posted to the transaction table.", vbOKOnly, "QCD: Post Monthlies"
    Else
        MsgBox "Invoices failed to post to the transaction table.  Contact support.", vbOKOnly, "QCD: Post Monthlies"
        GoTo PROC_EXIT
    End If
        
    ' data transferred OK?
    If DidAllInvoicesPosttoPlat() Then
        ' nothing
    Else
        MsgBox "There are issues with the post.  Contact support." & vbCrLf & "Contact support.", vbOKOnly, "QCD: Post Monthlies"
        GoTo PROC_EXIT
    End If
    
    ' create csv file
    ' for importing into QuickBooks
    If Export_Invoice_Data("usp_InvoicePostIndivAll_csv", dteInvoiceDate, strUserName) Then
        MsgBox "Sales receipts export succeeded.", vbOKOnly, "QCD: Post Monthlies"
    Else
        MsgBox "Sales receipts export failed. Contact support.", vbOKOnly, "QCD: Post Monthlies"
        GoTo PROC_EXIT
    End If
    
    'remove data from form
    DoCmd.OpenQuery "qdelDelInvHdr_All"
    Me.Requery
    Me.[INPUT_DATE].SetFocus
    
    DoCmd.SetWarnings True
    DoCmd.Hourglass False
    
    Exit Sub
    
PROC_EXIT:
    DoCmd.SetWarnings True
    DoCmd.Hourglass False
    Exit Sub
    
PROC_ERR:
    
    MsgBox "There was an error processing Individual Monthly sales receipts." & vbCrLf & _
    vbCrLf & "The error number is: " & Err.Number & " and the system Error message is: " & Err.DESCRIPTION, vbCritical, "Export Error"
    
    Resume PROC_EXIT
End Sub

Private Sub Form_Load()
    
    With Me
        .INPUT_DATE.SetFocus
    End With
    
End Sub


' --------------   saves the invoice date for further use   ---------
Private Sub INV_DATE_AfterUpdate()
    
    'Purpose:
    '
    'Programmer:
    'Date Created:
    'Date Updated:
    'Updated by:
    
    On Error GoTo PROC_ERR
    
    Dim strSQL As String
    
    DoCmd.Hourglass True
    DoCmd.SetWarnings False
    
    If IsNull(Me.INPUT_DATE) Or Me.INPUT_DATE = "" Then
        mdteInvoiceDate = #1/1/1900#
    Else
        mdteInvoiceDate = Me.INPUT_DATE
    End If
    
    ' save the date entered in INPUT_DATE
    Call SaveInvoiceDate(mdteInvoiceDate)
    
    DoCmd.SetWarnings True
    DoCmd.Hourglass False
    
    Exit Sub
    
PROC_EXIT:
    DoCmd.SetWarnings True
    DoCmd.Hourglass False
    Exit Sub
    
PROC_ERR:
    If Err.Number = 94 Then
        Resume Next
    Else
        MsgBox "Error number " & Err.Number & ": " & Err.DESCRIPTION
        Resume PROC_EXIT
    End If
    
End Sub


Private Sub INV_DATE_LostFocus()
    
    Call INV_DATE_AfterUpdate
    
End Sub

