Private Sub Form_Load()
  On Error GoTo PROC_ERR
    Me![ReportList].SetFocus
    'SendKeys "{Down}"
  Exit Sub

PROC_ERR:
  MsgBox "The following error occured: " & Error$
  Resume Next
End Sub

'----------------------------------------------------------------------
' Preview
'
'   Purpose: View the selected report in print preview.
'
'   Returns: Nothing
'
'   Comments: None
'----------------------------------------------------------------------
Private Function Preview()

On Error GoTo Preview_Err

    
    Dim strFormName As String
    
    If IsNull(Me![ReportList]) Or Me![ReportList] = "" Then
        MsgBox "You must select a report", 16, "Error"
        Exit Function
    End If

    If IsNull(Me![ReportList].Column(3)) Or Me![ReportList].Column(3) = "" Then   'col 3 = criteria form
        DoCmd.OpenReport Me![ReportList].Column(2), A_PREVIEW
        Forms![fmnuReportBilling].Visible = False
        Forms![fmnuSwitchboard].Visible = False
    Else
        strFormName = Me![ReportList].Column(3)
        
        'DoCmd.OpenForm Me![ReportList].Column(3), A_NORMAL
        DoCmd.OpenForm strFormName, A_NORMAL
        Forms![fmnuReportBilling].Visible = False
        Forms(strFormName).SetFocus
        'DoCmd.Close A_FORM, "fmnuReportBilling"
        'DoCmd.Close A_FORM, "fdlgAA_AuditReport"
    End If

Preview_Exit:
    Exit Function

Preview_Err:
        MsgBox Error$
    Resume Preview_Exit

End Function

'----------------------------------------------------------------------
' ShowReports
'
'   Purpose: Print the selected report.
'
'   Returns: Nothing
'
'   Comments: None
'----------------------------------------------------------------------
Private Function ShowReports()
On Error GoTo Reports_Err

    If IsNull([ReportList]) Or [ReportList] = "" Then
        MsgBox "You must select a report", 16, "Error"
        DoCmd.GoToControl "ReportList"
        Exit Function
    End If

    DoCmd.OpenReport Me![ReportList].Column(2), A_NORMAL

Reports_Exit:
    Exit Function

Reports_Err:

    If Err = 2202 Then
        MsgBox Error$
    Else
        MsgBox ("Printing report was Cancelled")
    End If
    Resume Reports_Exit


End Function