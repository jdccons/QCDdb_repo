Private Sub Command2_Click()

'Purpose:  checks if record exits in tblGrowth
'          or if record exists and BeginEligEmps > 0 then bypass
'          else insert BeginEligEmps and CurrentEligEmps into
'          controls on subform
'John Criswell 3/4/00

On Error GoTo Command2_Click_Err

    Dim intEligEmps As Integer
    Dim strYear As String
    Dim strGroupID As String

    strYear = CStr(Year(date))
    strGroupID = Me.OpenArgs
    
    If IsThereGrowth(strGroupID, strYear) Or IsThereEligEmps(strGroupID, strYear) Then
        MsgBox "There are already records in tblGrowth for this group."
        DoCmd.Close
    Else
        intEligEmps = Me![txtEligEmps]

        Forms![frmGrp]![Child150].Form![Year] = strYear
        Forms![frmGrp]![Child150].Form![BeginEligEmps] = intEligEmps
        Forms![frmGrp]![Child150].Form![CurrentEligEmps] = intEligEmps

        DoCmd.Close
    End If

Command2_Click_Exit:
Exit Sub

Command2_Click_Err:
    
    MsgBox "Error " & Err.Number & ": " & Err.DESCRIPTION
    GoTo Command2_Click_Exit

End Sub
