Private Sub cboAfter_AfterUpdate()
    Dim strStart As String
    Dim strYear As String
    Dim datFilter As Date
    Dim datStart As Date
    Dim fBegin As Boolean
    Dim datEnd As Date
    
    strStart = Me.cboAfter
    If IsNull(Me.txtYear) Then
        MsgBox "You must enter the Year before you can proceed!", vbExclamation, "Missing Year"
        Exit Sub
    Else
        strYear = Me.txtYear
    End If
    
    Select Case strStart
    Case Is = "JAN 1"
        datStart = Format("1/1/" & strYear, "mm/dd/yyyy")
        'datEnd = Format("1/31/" & strYear, "mm/dd/yyyy")
        With Me
          .txtAfterDate = datStart
          .txtBetweenEnd = ""
          .txtBetweenStart = ""
        End With
    Case Is = "FEB 1"
        datStart = Format("2/1/" & strYear, "mm/dd/yyyy")
        With Me
          .txtAfterDate = datStart
          .txtBetweenEnd = ""
          .txtBetweenStart = ""
        End With
    Case Is = "MAR 1"
        datStart = Format("3/1/" & strYear, "mm/dd/yyyy")
        With Me
          .txtAfterDate = datStart
          .txtBetweenEnd = ""
          .txtBetweenStart = ""
        End With
    Case Is = "APR 1"
        datStart = Format("4/1/" & strYear, "mm/dd/yyyy")
        With Me
          .txtAfterDate = datStart
          .txtBetweenEnd = ""
          .txtBetweenStart = ""
        End With
    Case Is = "MAY 1"
        datStart = Format("5/1/" & strYear, "mm/dd/yyyy")
        With Me
          .txtAfterDate = datStart
          .txtBetweenEnd = ""
          .txtBetweenStart = ""
        End With
    Case Is = "JUN 1"
        datStart = Format("6/1/" & strYear, "mm/dd/yyyy")
        With Me
          .txtAfterDate = datStart
          .txtBetweenEnd = ""
          .txtBetweenStart = ""
        End With
    Case Is = "JUL 1"
        datStart = Format("7/1/" & strYear, "mm/dd/yyyy")
        With Me
          .txtAfterDate = datStart
          .txtBetweenEnd = ""
          .txtBetweenStart = ""
        End With
    Case Is = "AUG 1"
        datStart = Format("8/1/" & strYear, "mm/dd/yyyy")
        With Me
          .txtAfterDate = datStart
          .txtBetweenEnd = ""
          .txtBetweenStart = ""
        End With
    Case Is = "SEP 1"
        datStart = Format("9/1/" & strYear, "mm/dd/yyyy")
        With Me
          .txtAfterDate = datStart
          .txtBetweenEnd = ""
          .txtBetweenStart = ""
        End With
    Case Is = "OCT 1"
        datStart = Format("10/1/" & strYear, "mm/dd/yyyy")
        With Me
          .txtAfterDate = datStart
          .txtBetweenEnd = ""
          .txtBetweenStart = ""
        End With
    Case Is = "NOV 1"
        datStart = Format("11/1/" & strYear, "mm/dd/yyyy")
        With Me
          .txtAfterDate = datStart
          .txtBetweenEnd = ""
          .txtBetweenStart = ""
        End With
    Case Is = "DEC 1"
        datStart = Format("12/1/" & strYear, "mm/dd/yyyy")
        With Me
          .txtAfterDate = datStart
          .txtBetweenEnd = ""
          .txtBetweenStart = ""
        End With
    Case Else
        MsgBox "There is an error in the date, please select again", vbExclamation, "Processing Error"
        Exit Sub
    End Select
    
End Sub

Private Sub cboBetweenEnd_AfterUpdate()
