Private Sub cmdYearAll_Click()
    ' Process the list of groups
    '
    Dim strSQL As String
    Dim strSQLIns As String
    Dim strSQLchk As String
    Dim rst As DAO.Recordset
    Dim rst2 As DAO.Recordset
    Dim dbs As DAO.Database
    Dim datDate As Date
    Dim datEOM As Date
    Dim strGroup As String
    Dim strYear As String
    Dim dblInvAmt As Double
    Dim lngSubCnt As Long
    Dim lngDepCnt As Long
    Dim lngSubWDepCnt As Long
    Dim lngElegEmp As Long
    Dim intRet As Integer
    
    Set dbs = CurrentDb()
     
   
    With Me
       .txtMessage.Visible = True
       .txtMessage.BackColor = 16776960
       .txtMessage = "Processing, Please Wait!"
       .Refresh
       datDate = Format(.txtDateAll, "mm/dd/yyyy")
       datEOM = Format(.txtEOMAll, "mm/dd/yyyy")
       strYear = .txtYear
    End With
    DoCmd.Hourglass True
    
    strSQL = "SELECT tblGrp.GroupID, tblGrp.GRname, tblGrp.GRcancelled"
    strSQL = strSQL & " FROM tblGrp"
    strSQL = strSQL & " WHERE (((tblGrp.GRcancelled) = No))"
    strSQL = strSQL & " ORDER BY tblGrp.GroupID;"
    Set rst = dbs.OpenRecordset(strSQL, dbOpenSnapshot)
    If rst.RecordCount > 0 Then
     With rst
      Do While Not rst.EOF
        strGroup = !GROUPid
        ' Check for existing records
        strSQLchk = "SELECT tblGrowth.GroupID, tblGrowth.Year, tblGrowth.BeginEligEmps,"
        strSQLchk = strSQLchk & " tblGrowth.BeginSubscriberCount"
        strSQLchk = strSQLchk & " FROM tblGrowth"
        strSQLchk = strSQLchk & " WHERE (((tblGrowth.GroupID)='" & strGroup & "') "
        strSQLchk = strSQLchk & " AND ((tblGrowth.Year)='" & strYear & "'));"
        Set rst2 = dbs.OpenRecordset(strSQLchk, dbOpenSnapshot)
        If rst2.RecordCount > 0 Then
            ' If group Exists prompt to replace
            intRet = MsgBox("The group " & strGroup & " already exists on the Growth table for this year.  Do you want to replace it? (Press Cancel to Stop all processing)", vbInformation + vbYesNoCancel, "Data Exists")
            If intRet = vbCancel Then
                With Me
                   .txtMessage.BackColor = 8454143
                   .txtMessage = "Processing Cancelled!"
                   .Refresh
                End With
                DoCmd.Hourglass False
                MsgBox "Operation Cancelled!", vbInformation, "Stopped"
                Exit Sub
            ElseIf intRet = vbYes Then
                lngElegEmp = GetEECnt(strGroup, strYear) ' From tblGrowth
                lngSubCnt = GetSubscriberCnt(strGroup)   ' From tblSubscr
                lngDepCnt = GetDependentCnt(strGroup)    ' From tblSubscr
                lngSubWDepCnt = GetSubscriberWDepCnt(strGroup)  ' From tblSubscr
                dblInvAmt = CalcCurrentInvoice(strGroup, datDate, datEOM) ' From Date Range on ARTRAN
                strSQLIns = "UPDATE tblGrowth SET tblGrowth.CurrentEligEmps = " & lngElegEmp & ", "
                strSQLIns = strSQLIns & " tblGrowth.CurrentSubscriberCount =" & lngSubCnt & ", "
                strSQLIns = strSQLIns & " tblGrowth.CurrentDependentCount =" & lngDepCnt & ", "
                strSQLIns = strSQLIns & " tblGrowth.CurrentSubscrswithDeps =" & lngSubWDepCnt & ", "
                strSQLIns = strSQLIns & " tblGrowth.CurrentInvoiceAmt =" & dblInvAmt & ", "
                strSQLIns = strSQLIns & " tblGrowth.CurrentInvoiceDate = #" & datDate & "#"
                strSQLIns = strSQLIns & " WHERE (((tblGrowth.GroupID)='" & strGroup & "')"
                strSQLIns = strSQLIns & " AND ((tblGrowth.Year)='" & strYear & "'));"
                dbs.Execute strSQLIns
            End If
        Else
            ' Insert new record
            lngElegEmp = GetEECnt(strGroup, strYear) ' From tblGrowth
            lngSubCnt = GetSubscriberCnt(strGroup)   ' From tblSubscr
            lngDepCnt = GetDependentCnt(strGroup)    ' From tblSubscr
            lngSubWDepCnt = GetSubscriberWDepCnt(strGroup)  ' From tblSubscr
            dblInvAmt = CalcCurrentInvoice(strGroup, datDate, datEOM) ' From Date Range on ARTRAN
            strSQLIns = "INSERT INTO tblGrowth ( BeginEligEmps, BeginSubscriberCount, BeginDependentCount, "
            strSQLIns = strSQLIns & " BeginSubscrswithDeps, BeginInvoiceAmt, BeginInvoiceDate, GroupID, Year ) VALUES "
            strSQLIns = strSQLIns & " ( " & lngElegEmp & ", "
            strSQLIns = strSQLIns & " " & lngSubCnt & ", "
            strSQLIns = strSQLIns & " " & lngDepCnt & ", "
            strSQLIns = strSQLIns & " " & lngSubWDepCnt & ", "
            strSQLIns = strSQLIns & " " & dblInvAmt & ", "
            strSQLIns = strSQLIns & " #" & datDate & "#,"
            strSQLIns = strSQLIns & " '" & strGroup & "',"
            strSQLIns = strSQLIns & " '" & strYear & "' );"
            dbs.Execute strSQLIns
        End If
        rst2.Close
        .MoveNext
      Loop
     End With
    End If
     
    With Me
       .txtMessage.BackColor = 8454143
       .txtMessage = "Processing, Complete!"
       .Refresh
    End With
    
   strSQL = "SELECT tblDateTimeTracking.ReportDateTime"
   strSQL = strSQL & " FROM tblDateTimeTracking;"
   Set rst = dbs.OpenRecordset(strSQL, dbOpenSnapshot)
   If rst.RecordCount > 0 Then
     With rst
        datDate = !ReportDateTime
        Me.txtDateTime = datDate
     End With
   Else
      Me.txtDateTime = "Last Date/Time Not recorded"
   End If
   
    DoCmd.Hourglass False
    rst.Close
    Set dbs = Nothing
     
End Sub

Private Sub Form_Load()
   Dim datFilter As Date
   Dim datDate As Date
   Dim strSQL As String
   Dim rst As DAO.Recordset
   Dim dbs As DAO.Database
   Dim strYear As String
   Dim strMonth As String
   Dim strList As String
   Dim strGEOlist As String
        
        strYear = Format(Date, "YYYY")
        strMonth = Month(Date)
        With Me
        Select Case strMonth
        Case Is = "1"
            .cboStartDate = "JAN 1"
        Case Is = "2"
            .cboStartDate = "FEB 1"
        Case Is = "3"
            .cboStartDate = "MAR 1"
        Case Is = "4"
            .cboStartDate = "APR 1"
        Case Is = "5"
            .cboStartDate = "MAY 1"
        Case Is = "6"
            .cboStartDate = "JUN 1"
        Case Is = "7"
            .cboStartDate = "JUL 1"
        Case Is = "8"
            .cboStartDate = "AUG 1"
        Case Is = "9"
            .cboStartDate = "SEP 1"
        Case Is = "10"
            .cboStartDate = "OCT 1"
        Case Is = "11"
            .cboStartDate = "NOV 1"
        Case Is = "12"
            .cboStartDate = "DEC 1"
        End Select
        End With
        datFilter = Format("1/1/" & strYear, "mm/dd/yyyy")
        
    Set dbs = CurrentDb()
    
    With Me
       .txtFilterDate = datFilter
       .Refresh
       .Requery
       .txtMessage.BackColor = 16777215
       .txtMessage = ""
       .txtYear = Format(Date, "YYYY")
       cboStartDate_AfterUpdate
       fraSelGroups_AfterUpdate
       fraSelDate_AfterUpdate
       .txtGroups = "All Groups"
       fAllGroups = True
    End With
    
    'load client service reps into combo box
    strSQL = "SELECT tlkpClientServiceReps.RepID, tlkpClientServiceReps.RepName, tlkpClientServiceReps.IsRepActive "
    strSQL = strSQL & "FROM tlkpClientServiceReps "
    strSQL = strSQL & "WHERE (((tlkpClientServiceReps.IsRepActive)=Yes));"
    
    Set rst = dbs.OpenRecordset(strSQL, dbOpenSnapshot)
    strList = "All, All Client Service Reps;"
    If rst.RecordCount > 0 Then
      With rst
       Do While Not rst.EOF
        strList = strList & !RepID & "," & !RepName & ";"
        Me.cboAcctMgr.RowSource = strList
        .MoveNext
       Loop
      End With
    End If
      
    strSQL = "SELECT DISTINCTROW tblGeographical.GEOid, tblGeographical.GEOdesc"
    strSQL = strSQL & " FROM tblGeographical;"
    Set rst = dbs.OpenRecordset(strSQL, dbOpenSnapshot)
    strGEOlist = "All, All GEO Id's;"
    If rst.RecordCount > 0 Then
      With rst
       Do While Not rst.EOF
        strGEOlist = strGEOlist & !GEOid & "," & !GEOdesc & ";"
        Me.cboGEOid.RowSource = strGEOlist
        .MoveNext
       Loop
      End With
    End If
    
   strSQL = "SELECT tblDateTimeTracking.ReportDateTime"
   strSQL = strSQL & " FROM tblDateTimeTracking;"
   Set rst = dbs.OpenRecordset(strSQL, dbOpenSnapshot)
   If rst.RecordCount > 0 Then
     With rst
        datDate = !ReportDateTime
        Me.txtDateTime = datDate
     End With
   Else
      Me.txtDateTime = "Last Date/Time Not recorded"
   End If

    rst.Close
    Set dbs = Nothing
End Sub

