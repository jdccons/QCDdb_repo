Private Function CheckFilter(strFilter As String) As Boolean
    Dim strSQL As String
    Dim rst As DAO.Recordset
    Dim dbs As DAO.Database
    Set dbs = CurrentDb()
    
    strSQL = "SELECT tblGrowth.*, *"
    strSQL = strSQL & " FROM (tblGrowth INNER JOIN tblGrp ON tblGrowth.GroupID = tblGrp.GroupID) "
    'strSQL = strSQL & " INNER JOIN tblAcctMgr ON tblGrp.GRacctMgr = tblAcctMgr.AcctMgrID"
    'WHERE (((tblGrowth.BeginInvoiceDate)=#1/1/2000#) AND (([GRacctMgr])=1));
    strSQL = strSQL & " WHERE " & strFilter
       
    Set rst = dbs.OpenRecordset(strSQL, dbOpenSnapshot)
    If rst.RecordCount > 0 Then
        CheckFilter = True
    Else
        CheckFilter = False
    End If
     
    rst.Close
    Set dbs = Nothing
End Function