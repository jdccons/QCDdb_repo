Option Compare Database

Private Sub cmdAdd_Click()
    
    With Me
        ExecuteSQL "EXEC uspAddCoverage '" & .CoverCode & "','" & .CoverDescr & "';"
        .lstCoverages.Requery
        .CoverID = ""
        .CoverCode = ""
        .CoverDescr = ""
    End With
End Sub

Private Sub cmdUpdate_Click()

    With Me
      If .CoverID <> "" Then
        ExecuteSQL "EXEC uspUpdateCoverage " & Me.CoverID & ",'" & Me.CoverCode & "','" & Me.CoverDescr & "';"
        .lstCoverages.Requery
        .CoverID = ""
        .CoverCode = ""
        .CoverDescr = ""
      Else
        MsgBox "Please select a Coverage from the list.", vbInformation, "Action Canceled"
      End If
    End With
End Sub

Private Sub lstCoverages_AfterUpdate()
    With Me
        .CoverID = .lstCoverages.Column(0)
        .CoverCode = .lstCoverages.Column(1)
        .CoverDescr = .lstCoverages.Column(2)
    End With
End Sub
Private Sub cmdClose_Click()
On Error GoTo Err_cmdClose_Click


    DoCmd.Close

Exit_cmdClose_Click:
    Exit Sub

Err_cmdClose_Click:
    MsgBox Err.DESCRIPTION
    Resume Exit_cmdClose_Click
    
End Sub