Private Sub Form_BeforeInsert(Cancel As Integer)
On Error GoTo Err_Form_BeforeInsert

   With Me
     .LogSize = gstrSize
     .LogPhoneLog = fPhone
   End With

Exit_Form_BeforeInsert:
    Exit Sub

Err_Form_BeforeInsert:
    MsgBox Err.DESCRIPTION
    Resume Exit_Form_BeforeInsert
End Sub

Private Sub Form_Open(Cancel As Integer)
   
On Error GoTo Err_Form_Open

   Dim strSQL As String
   Dim rst As DAO.Recordset
   Dim dbs As DAO.Database
   Dim strYear As String
   Dim strGroup As String
   
   Set dbs = CurrentDb()
   
   strYear = Year(Date)
   'strGroup = [Forms]![frmGrpLogSwitch].[GroupID]
   
   strSQL = "SELECT tblGrowth.GroupID, tblGrowth.Year, tblGrowth.BeginEligEmps, "
   strSQL = strSQL & " tblGrowth.CurrentEligEmps, IIf(Month(Date())=1,"
   strSQL = strSQL & " IIf([BeginEligEmps]>=1000,'A',"
   strSQL = strSQL & " IIf([BeginEligEmps]<1000 And [BeginEligEmps]>=100,'B',"
   strSQL = strSQL & " IIf([BeginEligEmps]<100 And [BeginEligEmps]>=30,'C+',"
   strSQL = strSQL & " IIf([BeginEligEmps]<30 And [BeginEligEmps]>=0,'Baby C','?')))),"
   strSQL = strSQL & " IIf([CurrentEligEmps]>=1000,'A',"
   strSQL = strSQL & " IIf([CurrentEligEmps]<1000 And [CurrentEligEmps]>=100,'B',"
   strSQL = strSQL & " IIf([CurrentEligEmps]<100 And [CurrentEligEmps]>=30,'C+',"
   strSQL = strSQL & " IIf([CurrentEligEmps]<30 And [CurrentEligEmps]>=0,'Baby C','NA'))))) AS Rating"
   strSQL = strSQL & " FROM tblGrowth"
   strSQL = strSQL & " WHERE (((tblGrowth.GroupID)='" & strGroup & "') "
   strSQL = strSQL & " AND ((tblGrowth.Year)='" & strYear & "'));"
   
   Set rst = dbs.OpenRecordset(strSQL, dbOpenSnapshot)
   
   If rst.RecordCount > 0 Then
        With rst
            gstrSize = !Rating
            fPhone = False
        End With
   'If the group does not exist in tblGrowth, cannot calculate logsize
   Else
        gstrSize = "NA"
        fPhone = False
   End If
   
   rst.Close
   Set dbs = Nothing
   
Exit_Form_Open:
    Exit Sub

Err_Form_Open:
    MsgBox Err.DESCRIPTION
    Resume Exit_Form_Open
    
End Sub
