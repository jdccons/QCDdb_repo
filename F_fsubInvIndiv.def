Private Sub Form_GotFocus()

  On Error GoTo PROC_ERR
    Dim dbsDb As DAO.Database
    Dim rstRs As DAO.Recordset
    Dim strWhere$
    Dim strCriterion$

    strCriterion = Forms![f-INVOICEsub]![CUST KEY]
    strWhere = "[PltCustKey] = '" & strCriterion & "'"

    Set dbsDb = DBEngine(0)(0)
    Set rstRs = dbsDb.OpenRecordset("tblSubscr", dbOpenDynaset, dbSeeChanges)
    rstRs.FindFirst strWhere
     Forms![f-INVOICEsub]![ITEM KEY] = rstRs("SUBssn")
    'Forms![f-INVOICE]![ADDRESS 1] = rs("GRstreet1")
    'Forms![f-INVOICE]![ADDRESS 2] = rs("GRstreet2")
    'Forms![f-INVOICE]![City] = rs("GRcity")
    'Forms![f-INVOICE]![State] = rs("GRstate")
    'Forms![f-INVOICE]![Zip CODE] = rs("GRzip")
    'Forms![f-INVOICE]![ATTENTION] = rs("GRmainCONT")
    'Forms![f-INVOICE]![TERRITORY KEY] = rs("GRGeoID")
    'Forms![f-INVOICE]![SALESP    KEY] = rs("GRagentID1")

    rstRs.Close
    dbsDb.Close

  Exit Sub

