Private Sub Form_BeforeUpdate(intCancel As Integer)

On Error GoTo Err_Form_BeforeUpdate
    
    Dim strSSN As String
    Dim strGroupID As String
    Dim intPlanID As String
    Dim intCntOfCoverages As Integer
        
    If fDelete = True Then
        intCancel = True
        GoTo Exit_Form_BeforeUpdate
    End If
    
    With Me
        strGroupID = .SubGroupID
    
        If strGroupID <> "ALDNE" Or strGroupID <> "NSISD" Or strGroupID <> "AMSTR" Then
        
            'Check to see if the user has changed the subscriber's SSN (Primary Key)
            'If the SSN has been changed, then the foreign key on the dependents'
            'table must be updated.  Must allow database cascade update to run freely.
            strSSN = Nz(.SUBssn)
            If strSSN = mstrSSN Then
            
                'check to make sure the dob for the subscriber has been populate
                'only if the group type is All American
                If .OpenArgs = 4 Then ' all american ?
                    If IsNull(.SUBdob) Or .SUBdob = "" Then
                        MsgBox "You must populate the DOB field."
                        intCancel = True
                        .SUBdob.SetFocus
                        GoTo Exit_Form_BeforeUpdate
                    End If
                End If
                
                'check to make sure the gender for the subscriber has been populated
                'only if the group type is All American
                If .OpenArgs = 4 Then ' all american ?
                    If IsNull(.SubGender) Or .SubGender = "" Then
                        MsgBox "You must populate the gender field."
                        intCancel = True
                        .SubGender.SetFocus
                        GoTo Exit_Form_BeforeUpdate
                    End If
                End If
                
                'check to see if plan has been selected
                If .OpenArgs = 4 Then ' all american ?
                    If IsNull(.cboPlan) Or .cboPlan = "" Then
                        MsgBox "You must enter a plan!"
                        intCancel = True
                        .cboPlan.SetFocus
                        GoTo Exit_Form_BeforeUpdate
                    End If
                End If
                
                'check to see if coverage has been selected
                If .OpenArgs = 4 Then ' all american ?
                    If IsNull(.cboSUBcoverage) Or .cboSUBcoverage = "" Then
                        MsgBox "You must enter a coverage type!"
                        intCancel = True
                        .cboSUBcoverage.SetFocus
                        GoTo Exit_Form_BeforeUpdate
                    End If
                End If
                
                'check to see if effective date has been entered
                If Me.OpenArgs = 4 Then ' all american ?
                    If IsNull(.SubEffDate) Or .SubEffDate = "" Then
                        MsgBox "You must enter an effective date."
                        intCancel = True
                        .SubEffDate.SetFocus
                        GoTo Exit_Form_BeforeUpdate
                    End If
                End If
                
                'validate deps and plans
                If Me.OpenArgs = 4 Then  ' all american ?
                    intPlanID = .cboPlan.Column(1)
                    LoadADORecordSet "uspTierType '" & strGroupID & "', " & intPlanID
                    If Not g_adoRS.EOF Then
                      intCntOfCoverages = g_adoRS("CntOfCoverages")
                      g_adoRS.Close
                    Else
                      intCntOfCoverages = 0
                      g_adoRS.Close
                    End If
                    
                    If ValidateDepsWithPlan(strSSN, intCntOfCoverages) Then
                        intCancel = False
                    Else
                        intCancel = True
                        .cboSUBcoverage.SetFocus
                        GoTo Exit_Form_BeforeUpdate
                    End If
                    
                
                End If ' all american ?
            
            End If 'has ssn been changed ?
        
        End If ' if not ALDINE etc ?
        
    End With
    
    
Exit_Form_BeforeUpdate:
    fDelete = False
    Exit Sub

Err_Form_BeforeUpdate:
    MsgBox Error$
    Resume Exit_Form_BeforeUpdate
    
End Sub
Private Sub cboSubCoverage_AfterUpdate()
'------------------------------------------------
'Purpose:  Populate rate control based on coverage
'          and plan id that applies to group
'          If group is All American and the option
'          is Blue, use zero for a rate
'------------------------------------------------
'Programmer:  John Criswell
'Create Date:
'Update Date:
'Updated by:

On Error GoTo Err_cboSubCoverage_AfterUpdate

    Dim strCoverage As String
    Dim intCoverID As Integer
    Dim intPlanID As Integer
    Dim strGroupID As String
    Dim strGroupPlanID As String
    
    'check to see if the rate ID for that subscriber exists
    'on the rate table
    'if it does not then
    'display message and exit sub
    'parameters:
    'SubGroupID
    'PlanID
    'CoverID
    Dim db As DAO.Database
    Dim rs As DAO.Recordset
    Dim strSQL As String
    
    Set db = CurrentDb()
        
    With Me
        strGroupID = .SubGroupID
        intPlanID = Nz(.cboPlan.Column(1))
        intCoverID = Nz(.cboSUBcoverage.Column(1))
    End With
    
    strSQL = "SELECT tblRates.GroupID, tblRates.PlanID, " & _
                "tblRates.CoverID " & _
                "FROM tblRates " & _
                "WHERE (((tblRates.GroupID)='" & strGroupID & "') " & _
                "AND ((tblRates.PlanID)= " & intPlanID & ") " & _
                "AND ((tblRates.CoverID)= " & intCoverID & "));"
    
    Set rs = db.OpenRecordset(strSQL, dbOpenSnapshot)
    If fDelete = True Then
        GoTo Exit_cboSubCoverage_AfterUpdate
    Else
        If rs.RecordCount > 0 Then
            With Me
                'populate no of dependents
                .SUBnoDEPENT = DependentCheck(Me.SUBssn)
                If .cboSUBcoverage <> "" And .cboPlan <> "" And .SubGroupID <> "" _
                Or Not IsNull(.cboSUBcoverage) And IsNull(.cboPlan) And Not IsNull(.SubGroupID) Then
                    'populate rate
                    .SUBrate = FindRate(strGroupID, intCoverID, intPlanID)
                    .RateID = lngRateID
                Else
                    .RateID = 0
                End If
            End With
            Call Form_BeforeUpdate(True)
        Else
            MsgBox "The Plan and Coverage for this subscriber has not been setup " & vbCrLf & _
                    "on the Group form.  Please go back and set up the Plans and " & vbCrLf & _
                    "Coverages that are needed for the group.", vbOKOnly, "No Plan and Coverage"
        End If
    End If
    
    rs.Close
    Set rs = Nothing
    db.Close
    Set db = Nothing

Exit_cboSubCoverage_AfterUpdate:
    fDelete = False
    Exit Sub

Err_cboSubCoverage_AfterUpdate:
    MsgBox Error$
    Resume Exit_cboSubCoverage_AfterUpdate
    
End Sub
