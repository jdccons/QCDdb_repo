Private Sub fsubGrpSubscrDep_Exit(intCancel As Integer)

  On Error GoTo PROC_ERR
  
    If fDelete = True Then
        Exit Sub
    Else
        If Forms![frmGrpSubscr]![SUBnoDEPENT] <> DCount("[DEPcounter]", "tblDependent", "[DEPsubID] = [SUBssn]") Then
            MsgBox "The Number of Dependents entered in the field above MUST equal the number of dependents listed here.  The No. Dep field above will be changed to " + Str$(DCount("[DEPcounter]", "tblDependent", "[DEPsubID] = [SUBssn]")) + "."
            Forms![frmGrpSubscr]![SUBnoDEPENT] = DCount("[DEPcounter]", "tblDependent", "[DEPsubID] = [SUBssn]")
        Else
        End If
    End If
    Exit Sub

