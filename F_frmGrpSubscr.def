CodeBehindForm
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database   'Use database order for string comparisons
Option Explicit

Const Indiv = 1
Const IndivPlusOne = 2
Const IndivPlusChild = 3
Const IndivPlusFamily = 4

Const FourTier = 4
Const ThreeTier = 3
Dim mstrSSN As String
Public Function ValidateDepsWithPlan(strSSN As String, intCntOfCoverages As Integer) As Boolean
    ' Comments  :
    ' Parameters: strSSN -
    ' Modified  :
    '
    ' --------------------------------------------------
    
    On Error GoTo ErrorHandler
    
    Dim Subscr As clsSubscr
    Dim intDepCnt As Integer
    Dim intSpouseCnt As Integer
    Dim intOtherCnt As Integer
    
    'get key information on the subscriber
    Set Subscr = New clsSubscr
    'load SSN in Subscr object
    Subscr.SSN = strSSN
    
    'get data about subscriber from Subscr class
    intDepCnt = Subscr.DepCnt
    intSpouseCnt = Subscr.SpouseCnt
    intOtherCnt = Subscr.OtherCnt
        
    'check to see that the number of dependents is consistent with the plan type
    
    If intCntOfCoverages = 4 Then
        'four tier plan
        Select Case Me.cboSUBcoverage
            
            Case Indiv
                If intDepCnt <> 0 Then
                    MsgBox "You have selected Individual Only coverage " & vbCrLf & _
                    "and you have also created dependents. " & vbCrLf & vbCrLf & _
                    "You must change the coverage or the dependents."
                    ValidateDepsWithPlan = False
                End If
            Case IndivPlusOne
                If intDepCnt > 1 Or _
                    intDepCnt = 1 And intSpouseCnt = 0 And intOtherCnt <> 1 Or _
                    intDepCnt = 1 And intSpouseCnt <> 1 And intOtherCnt = 0 Then
                    MsgBox "You have selected Individual Plus One coverage and " & vbCrLf & _
                    "you have either no dependents or more than one dependent. " & vbCrLf & _
                    "Or you may have one child and no spouse or 'other relationship'. " & vbCrLf & vbCrLf & _
                    "You must change the coverage or the dependents."
                    ValidateDepsWithPlan = False
                End If
            Case IndivPlusChild
                If intDepCnt = 0 Or _
                    intDepCnt > 0 And intSpouseCnt <> 0 Or _
                    intDepCnt > 0 And intOtherCnt <> 0 Then
                    MsgBox "You have selected Individual Plus Child(ren) coverage and " & vbCrLf & _
                        "you have either no dependents or you have a spouse or other. " & vbCrLf & vbCrLf & _
                        "You must change the coverage or the dependents."
                    ValidateDepsWithPlan = False
                End If
            Case IndivPlusFamily
                If intDepCnt = 0 Or _
                    intDepCnt > 1 And intSpouseCnt = 0 And intOtherCnt <> 1 Or _
                    intDepCnt > 1 And intSpouseCnt <> 1 And intOtherCnt = 0 Then
                    MsgBox "You have selected Individual Plus Family coverage and " & vbCrLf & _
                        "you either have no dependents or you don't have a spouse " & vbCrLf & _
                        "or an other. " & vbCrLf & vbCrLf & _
                        "You must change the coverage or the dependents."
                    ValidateDepsWithPlan = False
                End If
            
            
        End Select
        ValidateDepsWithPlan = True
            
    Else
        'if the plan is not four tier then validate the dependents with the plans
         Select Case Me.cboSUBcoverage
            Case Indiv
                If intDepCnt <> 0 Then
                    MsgBox "The number of dependents and the plan type don't match."
                    ValidateDepsWithPlan = False
                End If
            Case IndivPlusOne
                If intDepCnt <> 1 Then
                    MsgBox "The number of dependents and the plan type don't match."
                    ValidateDepsWithPlan = False
                End If
            Case IndivPlusFamily
                If intDepCnt < 2 Then
                    MsgBox "The number of dependents and the plan type don't match."
                    ValidateDepsWithPlan = False
                End If
        End Select
        ValidateDepsWithPlan = True
        
    End If ' is four tier or not
    
    Set Subscr = Nothing
    
    
Exit_Point:
    Exit Function
    
ErrorHandler:
    ValidateDepsWithPlan = False
    MsgBox Err.Description & " " & Err.Number
    Resume Exit_Point
End Function
Public Function PrintCard() As Boolean
    ' Comments  :
    ' Parameters:  -
    ' Returns   : Boolean -
    ' Modified  :
    '
    ' --------------------------------------------------
    
    On Error GoTo ErrorHandler
    
    Call PrintMemberCard
    
    PrintCard = True
    
Exit_Point:
    Exit Function
    
ErrorHandler:
    PrintCard = False
    MsgBox Err.Description & " " & Err.Number
    Resume Exit_Point
    
End Function
Public Sub PrintMemberCard()
    ' Comments  :
    ' Parameters:  -
    ' Modified  :
    '
    ' --------------------------------------------------
    
    On Error GoTo ErrorHandler
    
    Dim strSQL
    Dim strSSN As String
    
    If IsNull(Forms![frmGrpSubscr]![SubEffDate]) Then
        MsgBox "The Effective Date on Subscriber Membership Cards cannot be blank." + Chr$(13) + "You must fill in this Subscriber's Effective Date before continuing.", 16, "MISSING Subscriber EFFECTIVE DATE"
    Else
        ' Save Record prior to printing card in case info. has been changed
        SendKeys "+{ENTER}"
        
        ' Print Member Card
        strSSN = Me![SUBssn]
        DoCmd.SetWarnings False
        
        'need to update tmpEDI_rpt
         ExecuteSQL "DELETE FROM tmpEDI_rpt"
            
         strSQL = "INSERT INTO tmpEDI_Rpt ( SUBssn , SUBfirstNAME , " & _
                        "SUBlastNAME , SUBStreet , SUBcity , SUBstate , " & _
                        "SUBzip , GroupID ) " & _
                        "SELECT tblSubscr.SubSSN , tblSubscr.SubFirstName ," & _
                        " tblSubscr.SubLastName , [SubStreet1] + ' ' + " & _
                        "[SubStreet2] AS Street , tblSubscr.SubCity , " & _
                        "tblSubscr.SubState , tblSubscr.SubZip , tblSubscr.SubGroupID " & _
                        "FROM tblSubscr " & _
                        "WHERE (((tblSubscr.SubSSN)='" & strSSN & "'))"
        ExecuteSQL strSQL
               
        DoCmd.OpenReport "rptCardMemGrp", , , "[SUBssn] = '" & strSSN & "'"
               
        'mark the subscriber record as card printed with date
        Forms![frmGrpSubscr]![SUBcardPRT] = 1
        Forms![frmGrpSubscr]![SUBcardPRTdte] = date
               
        'need to update tmpEDI_rpt
        ExecuteSQL "DELETE FROM tmpEDI_rpt"
        
        DoCmd.SetWarnings True
        MsgBox "Cards sent to the printer!", vbExclamation, "Cards Printed"
