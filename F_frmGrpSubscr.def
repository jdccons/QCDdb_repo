Private Sub SUBssn_BeforeUpdate(Cancel As Integer)
    
    On Error GoTo ErrorHandler

    Dim dbs As DAO.Database
    Dim rs As DAO.Recordset
    Dim strSSN As String
    
    strSSN = Me![SUBssn]
    
    Set dbs = CurrentDb()
    Set rs = dbs.OpenRecordset("SELECT tblSubscr.SUBssn FROM tblSubscr WHERE (((tblSubscr.SUBssn)='" & strSSN & "'));", dbOpenSnapshot)
    If rs.RecordCount > 0 Then
        MsgBox "SSN is already in the database."
        Cancel = True
    End If
    rs.Close
    dbs.Close
    Set dbs = Nothing
        
Exit_Point:
    Exit Sub
    
ErrorHandler:
    MsgBox Err.Description & " " & Err.Number
    Resume Exit_Point
    
End Sub