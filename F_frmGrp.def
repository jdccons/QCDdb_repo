             "WHERE ((tblRates.GroupID)='" & strGroupID & "') " & _
             "ORDER BY tblRates.PlanID, tblRates.CoverID;"
   
    Set rst = dbs.OpenRecordset(strSQL, dbOpenSnapshot)
    
