Private Sub Form_BeforeDelConfirm(Cancel As Integer, Response As Integer)
    
    Response = acDataErrContinue
    Cancel = ConfirmDelete

End Sub

Function ConfirmDelete() As Integer

    ' Check for active Accounts before Delete
    Dim db As DAO.Database
    Dim rst As DAO.Recordset
    Dim strSQL As String
    Dim strAgtID As String
    
    Set db = CurrentDb()
    DoCmd.SetWarnings True
    strAgtID = Me.AGENTid
    
    strSQL = "SELECT tblGrpAgt.AGENTid, Count(tblGrpAgt.GroupID) AS ActCnt " & _
             "FROM tblGrpAgt GROUP BY tblGrpAgt.AGENTid HAVING (((tblGrpAgt.AGENTid)='" & strAgtID & "'));"
    Set rst = db.OpenRecordset(strSQL, dbOpenSnapshot)
    If rst.RecordCount > 0 Then
        MsgBox "This Agent is active and assigned to a Group.  In order to delete this agent please remove them from all groups.", vbCritical, "QCD: Delete Validation Failure"
        ConfirmDelete = 1
    Else
        db.Execute "DELETE FROM tblAgent where AgentID = '" & strAgtID & "';"
        ConfirmDelete = 0
    End If
    
    Me.Requery
    
    rst.Close
    Set db = Nothing
    


End Function

Private Sub Form_BeforeUpdate(intCancel As Integer)

On Error GoTo ErrorHandler

    'validate controls
    If IsNull(Me.AGENCY) Or Me.AGENCY = "" Then
      If Me.optgrpType.Value = 1 Then
        MsgBox "You must populate Agency name.  Record will not be saved.", vbExclamation, "QCD: Error"
        intCancel = True
      End If
    End If

    If IsNull(Me.AGENTfirst) Or Me.AGENTfirst = "" Then
      If Me.optgrpType.Value = 2 Or Me.optgrpType.Value = 3 Or Me.optgrpType.Value = 4 Then
        MsgBox "You must populate First Name.  Record will not be saved.", vbExclamation, "QCD: Error"
        intCancel = True
      End If
    End If
    
    If IsNull(Me.AGENTlast) Or Me.AGENTlast = "" Then
      If Me.optgrpType.Value = 2 Or Me.optgrpType.Value = 3 Or Me.optgrpType.Value = 4 Then
        MsgBox "You must populate Last Name.  Record will not be saved.", vbExclamation, "QCD: Error"
        intCancel = True
      End If
    End If
    
    ' Populate the lookup name control
    If Me.optgrpType.Value = 1 Then
        Me.AGENT_LUname = Me.AGENCY
    Else
        With Me
            If IsNull(.AGENTfirst) Then
                .AGENT_LUname = .AGENTlast
            Else
                .AGENT_LUname = .AGENTlast & ", " & .AGENTfirst
            End If
        End With
    End If
    
    'Me![AGENT_LUname] = Me![AGENTlast] & ", " & Me![AGENTfirst]
    
    'validate more controls
    If IsNull(Me.AGENTSSN) Or Me.AGENTSSN = "" Then
      If Me.optgrpType.Value = 2 Or Me.optgrpType.Value = 3 Or Me.optgrpType.Value = 4 Then
        MsgBox "You must populate SSN.  Record will not be saved.", vbExclamation, "QCD: Error"
        intCancel = True
      End If
    End If
    
    
    
    If IsNull(Me.AGENTtaxID) Or Me.AGENTtaxID = "" Then
      If Me.optgrpType.Value = 1 Then
        MsgBox "You must populate Tax ID.  Record will not be saved.", vbExclamation, "QCD: Error"
        intCancel = True
      End If
    End If

    If IsNull(Me.GeoID) Or Me.GeoID = "" Then
        MsgBox "You must populate Geo ID.  Record will not be saved.", vbExclamation, "QCD: Error"
        intCancel = True
    End If

Exit_Point:
    Exit Sub
    
ErrorHandler:
    MsgBox Err.DESCRIPTION & " " & Err.Number
    Resume Exit_Point

End Sub

Private Sub Form_Current()

On Error GoTo Err_Form_Current
    
    With Me
        .[AlphaControl] = Null
        .[AGENTPickList] = Null
    End With

    Call optgrpType_AfterUpdate

    'Check to see if this is a new record or existing agent, if existing
    'lock AgentID field, so user cannot change it.
    With Me
        If Not IsNull(.[ctlAGENTid]) Then  'existing agent
            .[ctlAGENTid].Locked = True
            .[ctlAGENTid].BackColor = 12632256  'gray
            .[ctlAGENTid].SetFocus
            
        Else                               'new agent
            .[ctlAGENTid].Locked = False
            .[ctlAGENTid].BackColor = 16777215  'white
            .[ctlAGENTid].SetFocus
        End If
    End With
    
    'Check to see if the agent is an A or a P
    'If A then leave background white
    'if P then make background red
    'otherwise leave it white
    If Me.[Status] = "A" Then
        With Me
            .[Status].BackColor = 65280
        End With
    ElseIf Me.[Status] = "P" Then
        With Me
            .[Status].BackColor = 255
        End With
    Else
        With Me
            .[Status].BackColor = 12632256
        End With
    End If
    
Exit_Form_Current:
    Exit Sub

Err_Form_Current:
    MsgBox Error$
    Resume Exit_Form_Current

End Sub

Private Sub optgrpType_AfterUpdate()
    
    On Error GoTo ErrorHandler

    With Me
        If .optgrpType.Value = 1 Then                          'agency
            .AGENTtaxID.Locked = False
            .AGENTtaxID.BackColor = 16777215    'white
            .AGENTSSN.Locked = True
            .AGENTSSN.BackColor = 12632256      'gray
        End If
        
        If .optgrpType.Value = 2 Or .optgrpType.Value = 3 Or .optgrpType.Value = 4 Then  'agent, independent agent, or employee
            .AGENTtaxID.Locked = True
            .AGENTtaxID.BackColor = 12632256    'white
            .AGENTSSN.Locked = False
            .AGENTSSN.BackColor = 16777215      'gray
        End If
    End With
    
Exit_Point:
    Exit Sub
    
ErrorHandler:
    MsgBox Err.DESCRIPTION & " " & Err.Number
    Resume Exit_Point
    
    
End Sub