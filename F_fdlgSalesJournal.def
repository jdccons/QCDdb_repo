Private Sub cmdClose_Click()

On Error GoTo Err_cmdClose_Click

    DoCmd.Close

Exit_cmdClose_Click:
    Exit Sub

Err_cmdClose_Click:
    MsgBox Err.DESCRIPTION
    Resume Exit_cmdClose_Click
    
End Sub

Private Sub btnReport_Click()
On Error GoTo Err_btnReport_Click

    Dim dteInvoiceDate As Date
    Dim lngReportType As Long
    Dim strReportType As String
    Dim strDocName As String
    Dim strWhere As String
    
    ' check for value in invoice date text box
    If IsNull(Me.Invoice_Date) Or Me.Invoice_Date = "" Then
        MsgBox "Please enter a value in the invoice date text box."
        GoTo Exit_btnReport_Click
    End If
   
    dteInvoiceDate = Me.Invoice_Date
    lngReportType = Me.Invoice_Type
    strDocName = "Sales Journal"
    
    Select Case lngReportType
        Case 1
            strReportType = "All American"
        Case 2
            strReportType = "QCD Only"
    End Select
    
    If lngReportType < 3 Then
        strWhere = "[Ship Date] = #" & dteInvoiceDate & "# " & "And " & "[Item] = '" & strReportType & "'"
    ElseIf lngReportType = 3 Then
        strWhere = "[Ship Date] = #" & dteInvoiceDate & "# " & "And [Item] In (" & """" & "All American" & """" & ", " & """" & "QCD Only" & """" & ")"
    End If
    
    DoCmd.SetWarnings False
       
    If IsReportOpen(strDocName) Then
        DoCmd.Close acReport, strDocName, acSaveNo
    Else
        DoCmd.OpenReport strDocName, acViewPreview, , strWhere
    End If
    
    DoCmd.SetWarnings True
    
    Exit Sub

Exit_btnReport_Click:
    Exit Sub

Err_btnReport_Click:
    MsgBox Err.DESCRIPTION
    Resume Exit_btnReport_Click
    
End Sub