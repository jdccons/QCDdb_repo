Private Sub cmdChange_Click()

'canceled status for individual subscribers
'1 - active
'2 - hold
'3 - canceled
'null - this should be the status for all group subscribers because they are neither active or cancelled.
'       if they are in the group they are active

  Dim intResp As Integer
  Dim intStat As Integer
  Dim lngColor As Long
  Dim strForm As String
  
  intResp = MsgBox("Are you sure you want to change the Status?", vbQuestion + vbYesNo, "QCD: Confirm")
  If intResp = vbYes Then
       Select Case Me.fraStatus
       Case Is = 1
        'Active to Cancel
        intStat = 3
        lngColor = 8421631
       Case Is = 2
        'Active to Hold
        intStat = 2
        lngColor = 10092543
       Case Is = 3
        'Cancel to Active
        intStat = 1
        lngColor = 16777215
       Case Is = 4
        'Cancel to Hold
        intStat = 2
        lngColor = 10092543
       Case Is = 5
        'Hold to Active
        intStat = 1
        lngColor = 16777215
       Case Is = 6
        'Hold to Cancel
        intStat = 3
        lngColor = 8421631
        
       End Select
       
       strForm = Me.txtForm
       
    With Forms(strForm)
       .txtSubCanceled = intStat
       .SUBlastNAME.BackColor = lngColor
       .SUBfirstNAME.BackColor = lngColor
       .SUBmiddleNAME.BackColor = lngColor
    End With
    
  End If
    
End Sub

Private Sub cmdClose_Click()
On Error GoTo Err_cmdClose_Click


    DoCmd.Close

Exit_cmdClose_Click:
    Exit Sub

Err_cmdClose_Click:
    MsgBox Err.DESCRIPTION
    Resume Exit_cmdClose_Click
    
End Sub