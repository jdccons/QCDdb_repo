Private Sub PreviewButton_Click()

    Dim strWhere As String
    
    Me.Visible = False
  
    ' Print Cancelled Individual Subscribers Only
    If (Me![Filter] = 1) Then
      strWhere = "[SUBstatus] = ""INDIV"" and [SUBcancelled] = 3"
      DoCmd.OpenReport "rptIndivSubscrCoverage", acViewPreview, , strWhere
    End If
 
    'Print all Individual Subscribers
    If (Me![Filter] = 2) Then
        strWhere = "[SUBstatus] = ""INDIV"""
        DoCmd.OpenReport "rptIndivSubscrCoverage", acViewPreview, , strWhere
    End If
    'Print Individual Subscribers who are monthly and haven't cancelled
    
    If (Me![Filter] = 3) Then
'        strWhere = "[PLANindTYPE] = ""MONTHLY"" and [SUBcancelled] <> 3"
'        DoCmd.OpenReport "rptIndivSubscrCoverage", acViewPreview, , strWhere
        DoCmd.OpenForm "fdlgPrtIndivSubscrCoverageCriteria", acNormal, , , , acWindowNormal
    End If
    
    'Print only Individual Subscribers with a yearly plan, that haven't cancelled
    If (Me![Filter] = 4) Then
        strWhere = "[GroupID] IN ""('INDV1','INDV2','INDV3')"" and [SUBcancelled] <> 3"
        DoCmd.OpenReport "rptIndivSubscrCoverage", acViewPreview, , strWhere
    ''DoCmd.ApplyFilter "qrptIndivSubscrCoverage", "[PLANindDESC] Like ""*Year*"""
  
     End If
End Sub