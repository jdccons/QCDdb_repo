            DoCmd.ApplyFilter "qfrmIndivSubscrCoverage", "[SUBGroupID] Like ""INDVM"" and [SUBcancelled]=1"
            .AllowAdditions = False
            .AllowDeletions = False
            .AllowEdits = False
            .ctlSUBssn.BackColor = 16777215
