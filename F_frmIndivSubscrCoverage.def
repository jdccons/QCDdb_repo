Private Sub Form_Current()

On Error GoTo PROC_ERR

        With Me
                If .ctlSUBcancelled = 2 Then
                    ' On Hold Customer change Name to Yellow
                    .ctlSUBfirstNAME.BackColor = 10092543
                    .ctlSUBlastNAME.BackColor = 10092543
                    .ctlSUBmiddleNAME.BackColor = 10092543
                ElseIf .ctlSUBcancelled = 3 Then
                    ' Canceled change Name to Red
                    .ctlSUBfirstNAME.BackColor = 8421631
                    .ctlSUBlastNAME.BackColor = 8421631
                    .ctlSUBmiddleNAME.BackColor = 8421631
                    '.ctlSUBcancelled.BackColor = 65280
                Else
                    ' Leave Name White
                    .ctlSUBfirstNAME.BackColor = 16777215
                    .ctlSUBlastNAME.BackColor = 16777215
                    .ctlSUBmiddleNAME.BackColor = 16777215
                    '.ctlSUBcancelled.BackColor = 16777215
                End If
                .Repaint
                If Not IsNull(.SUBssn) Then
                    .txtSelRec = .SUBssn
                Else
                    .txtSelRec = ""
                End If
        End With
    
    
PROC_EXIT:
  DoCmd.SetWarnings True
  DoCmd.Hourglass False
  Exit Sub


PROC_ERR:
  MsgBox "Error: (" & Err.Number & ") " & Err.DESCRIPTION, vbCritical, "Procedure: frmIndivSubscrCoverage.Form"
Resume PROC_EXIT
End Sub

Private Sub OptFilter_Click()

On Error GoTo PROC_ERR

    Dim strSQL As String
     
    'Display only records that are cancelled
            
    If (Me.OptFilter = 1) Then
        With Me
            .AllowAdditions = True
            .AllowDeletions = True
            .AllowEdits = True
            DoCmd.ApplyFilter "qfrmIndivSubscrCoverage", "[SUBcancelled]=3"
            .AllowAdditions = False
            .AllowDeletions = False
            .AllowEdits = False
            .ctlSUBssn.BackColor = 16777215
            .ctlSUBlastNAME.BackColor = 16777215
            .ctlSUBfirstNAME.BackColor = 16777215
            .ctlSUBmiddleNAME.BackColor = 16777215
            .ctlSUBrate.BackColor = 16777215
            .txtGROUPid.BackColor = 16777215
            .txtCoverage.BackColor = 16777215
