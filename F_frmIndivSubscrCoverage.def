            .txtCoverage.BackColor = 16777215
            .ctlSUBcontBEG.BackColor = 16777215
            .ctlSUBcontEND.BackColor = 16777215
            .ctlSUBbankDraftNo.BackColor = 16777215
            .ctlSUBcancelled.BackColor = 16777215
            .ctlSUBrate.BackColor = 16777215
        
        End With
        

    End If
 
    
    'Displays only yearly subscribers that have not been cancelled
    If (Me.OptFilter = 4) Then
        With Me
            .AllowAdditions = True
            .AllowDeletions = True
            .AllowEdits = True
            DoCmd.ApplyFilter "qfrmIndivSubscrCoverage", "[SUBGroupID] Like ""INDV*"" AND [SUBGroupID] NOT IN (""INDVM"") and [SUBcancelled]=1"
