Private Sub cboSUBPLANid_AfterUpdate()
On Error GoTo Err_SUBPLANid_AfterUpdate

    Dim Subscr As clsSubscr
    Dim intCoverID As Integer
    Dim intPlanID As Integer
    Dim strGroupID As String
    Dim strGroupPlanID As String
    
    Dim intDepCount
    Dim strSSN As String
    
    Set Subscr = New clsSubscr
   
   With Me
     strSSN = Nz(.SUBssn)
     Subscr.SSN = strSSN
     intDepCount = Subscr.DepCnt
     intCoverID = .CoverID
     strGroupID = .SUBgroupID
     .ctlSUBrate = FindRate(strGroupID, intCoverID, intPlanID)
     .RateID = lngRateID
   End With
   
   Set Subscr = Nothing
  
  Exit Sub

Exit_SUBPLANid_AfterUpdate:
    Exit Sub
    
Err_SUBPLANid_AfterUpdate:
    ' Display error information.
    MsgBox "Error number " & Err.Number & ": " & Err.DESCRIPTION
    GoTo Exit_SUBPLANid_AfterUpdate

End Sub

Private Sub cmdEditRec_Click()

    On Error GoTo PROC_ERR

    DoCmd.OpenForm "frmUpdateIndividualSubscriber", acNormal, , , , acWindowNormal
    With Forms("frmUpdateIndividualSubscriber")
        .ctlSUBssn = Me.txtSelRec
        .ctlSUBlastNAME = Me.ctlSUBlastNAME
        .ctlSUBfirstNAME = Me.ctlSUBfirstNAME
        .ctlSUBmiddleNAME = Me.ctlSUBmiddleNAME
        .cboGroupID = Me.SUBgroupID
        .SUBPLANid = Me.CoverID
        .ctlSUBrate = Me.ctlSUBrate
        .ctlSUBcontBEG = Me.ctlSUBcontBEG
        .ctlSUBcontEND = Me.ctlSUBcontEND
        .ctlSUBbankDraftNo = Me.ctlSUBbankDraftNo
        .ctlSUBcancelled = Me.ctlSUBcancelled
        .CoverID = Me.CoverID
        .RateID = Me.RateID
    End With
    
PROC_EXIT:
  DoCmd.SetWarnings True
  DoCmd.Hourglass False
  Exit Sub
  
PROC_ERR:
  MsgBox "Error: (" & Err.Number & ") " & Err.DESCRIPTION, vbCritical, "Procedure: frmIndivSubscrCoverage.cmdEditRec"
Resume PROC_EXIT
End Sub

