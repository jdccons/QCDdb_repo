Private Sub cmdClose_Click()
On Error GoTo Err_cmdClose_Click


    DoCmd.Close

Exit_cmdClose_Click:
    Exit Sub

Err_cmdClose_Click:
    MsgBox Err.DESCRIPTION
    Resume Exit_cmdClose_Click
    
End Sub

Private Sub cmdPickDate_Click()
    DoCmd.OpenForm "frmPickDate"
    With Forms("frmPickDate")
        !txtFormName = Me.Name
        !txtFillBox = "txtDate"
    End With
End Sub

Private Sub cmdViewReport_Click()
    
    If Me.txtDate = "" Or IsNull(Me.txtDate) = True Then
        MsgBox "You must enter a report date to continue.", vbExclamation, "Missing Date"
        Exit Sub
    End If
    
    DoCmd.OpenReport "rptAA_Invoice_Monthly", acViewPreview
    
End Sub